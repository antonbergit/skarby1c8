// РАСЧЕТ()
// Выполняет расчет вычисляемых показателей на заданной странице отчета
// (ячеек, выделенных зеленым цветом). 
// В случае неуказания листа, ничего происходить не будет
// Параметры:
//  ИмяСтраницыПанели - строка, имя страницы основной панели формы.
//
&НаКлиентеНаСервереБезКонтекста
Процедура Расчет(Форма, ИмяСтраницыПанели = "", ОбластьИмя = "")
	Перем ТаблицаБлока;
	
	Если (Форма.СтруктураРеквизитовФормы.мРежимПечати) Тогда
		Возврат; // служебный вызов в процессе печати
	КонецЕсли;
	
	Если (Форма.СтруктураРеквизитовФормы.ФлажокОтклАвтоРасчет) Тогда
		Возврат; // отключен режим авторасчета
	КонецЕсли;
	
	R0070G3 = ОкруглитьЧислоПоФормату(Форма, ПолучитьПоказательНаСервере("R0030G3", Форма, "ДекларацияПоНДФЛ"))
											+ ОкруглитьЧислоПоФормату(Форма, ПолучитьПоказательНаСервере("R0050G3", Форма, "ДекларацияПоНДФЛ"))
											- ОкруглитьЧислоПоФормату(Форма, ПолучитьПоказательНаСервере("R0060G3", Форма, "ДекларацияПоНДФЛ"));
	ЗаполнитьПоказательНаСервере("R0070G3", R0070G3, Форма, "ДекларацияПоНДФЛ");
											
	СуммаКорректировки = ОкруглитьЧислоПоФормату(Форма, ПолучитьПоказательНаСервере("R0100G3", Форма, "ДекларацияПоНДФЛ"))- ОкруглитьЧислоПоФормату(Форма, ПолучитьПоказательНаСервере("R0090G3", Форма, "ДекларацияПоНДФЛ"));
	Если СуммаКорректировки >= 0  Тогда
		R0111G3 = СуммаКорректировки;
		R0112G3 = 0;
		R0120G3 = СуммаКорректировки * ПолучитьПоказательНаСервере("R0120G1", Форма, "ДекларацияПоНДФЛ") / 100;
		ЗаполнитьПоказательНаСервере("R0111G3", R0111G3, Форма, "ДекларацияПоНДФЛ");
		ЗаполнитьПоказательНаСервере("R0112G3", R0112G3, Форма, "ДекларацияПоНДФЛ");
		ЗаполнитьПоказательНаСервере("R0120G3", R0120G3, Форма, "ДекларацияПоНДФЛ");
	Иначе	
		R0111G3 = 0;
		R0112G3 = - СуммаКорректировки;
		R0120G3 = 0;
		ЗаполнитьПоказательНаСервере("R0111G3", R0111G3, Форма, "ДекларацияПоНДФЛ");
		ЗаполнитьПоказательНаСервере("R0112G3", R0112G3, Форма, "ДекларацияПоНДФЛ");
		ЗаполнитьПоказательНаСервере("R0120G3", R0120G3, Форма, "ДекларацияПоНДФЛ");
	КонецЕсли;
	
	СуммаКорректировки = ОкруглитьЧислоПоФормату(Форма, ПолучитьПоказательНаСервере("R020G3", Форма, "Приложение"))- ОкруглитьЧислоПоФормату(Форма, ПолучитьПоказательНаСервере("R010G3", Форма, "Приложение"));
	Если СуммаКорректировки >= 0  Тогда
		R031G3 = СуммаКорректировки;
		R032G3 = 0;
		R040G3 = СуммаКорректировки * ПолучитьПоказательНаСервере("R040G1", Форма, "Приложение") / 100;
		ЗаполнитьПоказательНаСервере("R031G3", R031G3, Форма, "Приложение");
		ЗаполнитьПоказательНаСервере("R032G3", R032G3, Форма, "Приложение");
		ЗаполнитьПоказательНаСервере("R040G3", R040G3, Форма, "Приложение");
	Иначе	
		R031G3 = 0;
		R032G3 = - СуммаКорректировки;
		R040G3 = 0;
		ЗаполнитьПоказательНаСервере("R031G3", R031G3, Форма, "Приложение");
		ЗаполнитьПоказательНаСервере("R032G3", R032G3, Форма, "Приложение");
		ЗаполнитьПоказательНаСервере("R040G3", R040G3, Форма, "Приложение");
	КонецЕсли;
	
КонецПроцедуры // Расчет()

&НаКлиентеНаСервереБезКонтекста
Функция НомерСтраницыПриПечати(Форма, Знач ТекСтрока = 0)
	
	Перем СтруктураГруппы;
	
	спПарам = Форма.СтруктураРеквизитовФормы.спПараметрыСтраницы;
	
	КвоСтрокТаблицы = Форма.T1R.Количество();
	
	Если ТекСтрока = 0 Тогда
    	//так как последняя строка таблицы будет выводится вместе с подвалом, то количество страниц
		//совпадет со станицей, на которую попадет последняя строка
		ТекСтрока = КвоСтрокТаблицы;
    КонецЕсли;	
	
	КвоСтрокНаСтранице = спПарам.КвоСтрокНаСтранице;
	ВысотаОснШапки 	   = спПарам.ВысотаОснШапки;
	ВысотаДопШапки 	   = спПарам.ВысотаДопШапки;
	ВысотаПодвала 	   = спПарам.ВысотаПодвала;
	КвоСтрокВТаблицеЕслиСтраницаОдна = спПарам.КоличествоСтрокТаблицыНаЕдинстеннойСтранице;
	
	Если ТекСтрока <= КвоСтрокВТаблицеЕслиСтраницаОдна Тогда
		
		Возврат 1;
		
	ИначеЕсли ВысотаОснШапки + ВысотаДопШапки + ТекСтрока > КвоСтрокНаСтранице Тогда
		НомерСтраницы = 2 + Цел((ВысотаОснШапки + ВысотаДопШапки + ТекСтрока - КвоСтрокНаСтранице)/(КвоСтрокНаСтранице - ВысотаДопШапки)); 
	    ПустыеСтрокиНаСтранице = КвоСтрокНаСтранице - (ВысотаОснШапки + ВысотаДопШапки + ТекСтрока - КвоСтрокНаСтранице)%(КвоСтрокНаСтранице - ВысотаДопШапки);
	Иначе
		НомерСтраницы = 1;
		ПустыеСтрокиНаСтранице = КвоСтрокНаСтранице - ВысотаОснШапки - ВысотаДопШапки - ТекСтрока;	
	КонецЕсли;
	
	ОставшиесяСтрокиТаблицы = КвоСтрокТаблицы - ТекСтрока;
	
	Если (ПустыеСтрокиНаСтранице < ОставшиесяСтрокиТаблицы + ВысотаПодвала) 
		 И (ОставшиесяСтрокиТаблицы=0) Тогда
		//таблица с подвалом не поместится на этой странице
		//перенесем последнюю строку таблицы на следующую страницу
		Возврат НомерСтраницы+1
	Иначе 
		Возврат НомерСтраницы
	КонецЕсли
	
КонецФункции 

// ОкруглитьЧислоПоФормату(Форма, )
