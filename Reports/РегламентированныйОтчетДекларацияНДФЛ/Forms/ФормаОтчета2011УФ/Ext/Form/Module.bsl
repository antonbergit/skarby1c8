// Функция выполняет обновление по служебным данным
//
Функция ОбновитьПоСлужебнымДанным()

	ЧастноеЛицо = Истина;
	
	Если ЧастноеЛицо Тогда
		РазбитьПоКлеточкам(СтруктураДанныхДекларацияПоНДФЛ,"HTIN", "ИНН", 10);
		РазбитьПоКлеточкам(СтруктураДанныхДекларацияПоНДФЛ,"", "ЕДРПОУ", 8);
		РазбитьПоКлеточкам(СтруктураДанныхПриложение,"HTIN", "ИНН", 10);
		РазбитьПоКлеточкам(СтруктураДанныхПриложение,"", "ЕДРПОУ", 8);
		
		СтруктураДанныхДекларацияПоНДФЛ.HJ = "";
		СтруктураДанныхДекларацияПоНДФЛ.HF = "Х";
		СтруктураДанныхПриложение.HJ = "";
		СтруктураДанныхПриложение.HF = "Х";
	Иначе		
		РазбитьПоКлеточкам(СтруктураДанныхДекларацияПоНДФЛ,"", "ИНН", 10);
		РазбитьПоКлеточкам(СтруктураДанныхДекларацияПоНДФЛ,"HTIN", "ЕДРПОУ", 8);
		РазбитьПоКлеточкам(СтруктураДанныхПриложение,"", "ИНН", 10);
		РазбитьПоКлеточкам(СтруктураДанныхПриложение,"HTIN", "ЕДРПОУ", 8);
		
		СтруктураДанныхДекларацияПоНДФЛ.HJ = "Х";
		СтруктураДанныхДекларацияПоНДФЛ.HF = "";
		СтруктураДанныхПриложение.HJ = "Х";
		СтруктураДанныхПриложение.HF = "";
	КонецЕсли;
	
	РазбитьПоКлеточкам(СтруктураДанныхДекларацияПоНДФЛ,"HPASS", "Пасп", 8);
	РазбитьПоКлеточкам(СтруктураДанныхПриложение,"HPASS", "Пасп", 8);

	Для каждого ЭлементСтруктуры Из мСтруктураВариантыЗаполнения Цикл
	    Год   = Формат(СтруктураДанныхСлужебныеДанные.HZY,"ЧЦ=4;ЧГ=;");	
	    ГодОш = Формат(ЭтаФорма["СтруктураДанных" + ЭлементСтруктуры.Ключ]["HZYP"],"ЧЦ=4;ЧГ=;");
		Для НомЯчейки = 1 По 4 Цикл
			ЭтаФорма["СтруктураДанных" + ЭлементСтруктуры.Ключ]["Год" + НомЯчейки]   = Сред(Год, НомЯчейки, 1);
			ЭтаФорма["СтруктураДанных" + ЭлементСтруктуры.Ключ]["ГодОш" + НомЯчейки] = Сред(ГодОш, НомЯчейки, 1);
		КонецЦикла;
		
		Мес = СтруктураДанныхСлужебныеДанные.HZM;
		ЭтаФорма["СтруктураДанных" + ЭлементСтруктуры.Ключ]["Мес1"] = Сред(Формат(Мес, "ЧЦ=2; ЧВН="),1,1);
		ЭтаФорма["СтруктураДанных" + ЭлементСтруктуры.Ключ]["Мес2"] = Сред(Формат(Мес, "ЧЦ=2; ЧВН="),2,1);
		
		МесОш = Формат(ЭтаФорма["СтруктураДанных" + ЭлементСтруктуры.Ключ]["HZMP"],"ЧЦ=2; ЧДЦ=0; ЧВН=");		
		ЭтаФорма["СтруктураДанных" + ЭлементСтруктуры.Ключ]["МесОш1"] = Сред(МесОш,1,1);
		ЭтаФорма["СтруктураДанных" + ЭлементСтруктуры.Ключ]["МесОш2"] = Сред(МесОш,2,1);

	КонецЦикла;   
	
	РазбитьПоКлеточкам(СтруктураДанныхДекларацияПоНДФЛ,"HZIP", "Инд", 5);
	РазбитьПоКлеточкам(СтруктураДанныхДекларацияПоНДФЛ,"HINTURB", "КодТел", 5);
