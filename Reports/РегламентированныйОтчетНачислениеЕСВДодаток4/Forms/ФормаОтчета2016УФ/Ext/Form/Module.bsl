КонецПроцедуры

Процедура ВыгрузитьЛистВDBF(СтруктураПараметровЛиста)
	
	Перем ИмяСтраницы;
	Перем ИмяФайла;
	
	ВерсияВыгрузки = "";
	
	СтруктураПараметровЛиста.Свойство("ИмяСтраницы",  ИмяСтраницы);
	
	ДокЭкспорт = Новый XBase;
	ДокЭкспорт.Кодировка = КодировкаXBase.ANSI; //windows
	
	Если ИмяСтраницы = "ПереченьТаблиц" Тогда
		
		// не выгружаем
		СтруктураПараметровЛиста.Вставить("ИмяФайла", ИмяФайла);
		Возврат;
		
	ИначеЕсли ИмяСтраницы = "Таблица1" Тогда
		
		// не выгружаем
		СтруктураПараметровЛиста.Вставить("ИмяФайла", ИмяФайла);
		Возврат;
		
	ИначеЕсли ИмяСтраницы = "Таблица5" Тогда
		
		СтруктураГруппы = Неопределено;
		
		ДанныеФормы 	= ЭтаФорма[СтруктураРеквизитовФормы.мИдГруппы1];
		СтруктураГруппы = ДанныеФормыВЗначение(ДанныеФормы, Тип("ТаблицаЗначений"));
		ТекИдГруппы = СтруктураРеквизитовФормы.мИдГруппы1; 
	
		Если ТаблицаМногострочнойЧастиПуста(СтруктураГруппы, ТекИдГруппы) Тогда		
			ДокЭкспорт.ЗакрытьФайл();
			СтруктураПараметровЛиста.Вставить("ИмяФайла", ИмяФайла);
			Возврат;
		КонецЕсли;
		
		ИмяФайлаБезРасширения = "J30T405" + ВерсияВыгрузки;
		ИмяФайла              = ИмяФайлаБезРасширения+ ".DBF";
		
		ДокЭкспорт.поля.Добавить("PERIOD_M",	"N", 2);
		ДокЭкспорт.поля.Добавить("PERIOD_Y",	"N", 4);
		
		ДокЭкспорт.поля.Добавить("ROWNUM",		"N", 6);
		ДокЭкспорт.поля.Добавить("UKR_GROMAD",	"N", 1);
		
		ДокЭкспорт.поля.Добавить("ZO",			"N", 2);
		
		ДокЭкспорт.поля.Добавить("DOG_CPH",     "N", 1);
		
		ДокЭкспорт.поля.Добавить("NUMIDENT",	"C", 10);
		ДокЭкспорт.поля.Добавить("LN",			"C", 100);
		ДокЭкспорт.поля.Добавить("NM",			"C", 100);
		ДокЭкспорт.поля.Добавить("FTN",			"C", 100);
		ДокЭкспорт.поля.Добавить("START_DT",	"N", 2);
		ДокЭкспорт.поля.Добавить("END_DT",		"N", 2);
		
		ДокЭкспорт.поля.Добавить("NRM_DT",		"C", 8);
		
		ДокЭкспорт.поля.Добавить("PID_ZV",		"C", 150);
		
		
		ДокЭкспорт.СоздатьФайл(СтруктураРеквизитовФормы.КаталогДанных + "\" + ИмяФайла);
		ДокЭкспорт.АвтоСохранение = Истина;
		Если ДокЭкспорт.Первая() Тогда
			ДокЭкспорт.Очистить();
		КонецЕсли;
		
		КоличествоСтрок = 1;
		Для каждого СтрокаГруппы Из СтруктураГруппы Цикл
			
			ДокЭкспорт.Добавить();
			
			докЭкспорт.PERIOD_M 	= Месяц(СтруктураРеквизитовФормы.мДатаНачалаПериодаОтчета);
			докЭкспорт.PERIOD_Y 	= Год(СтруктураРеквизитовФормы.мДатаНачалаПериодаОтчета);
			
			докЭкспорт.ROWNUM 	= КоличествоСтрок;
			докЭкспорт.UKR_GROMAD 	= СтрокаГруппы["T1RXXXXG6"];
			
			докЭкспорт.ZO 	= СтрокаГруппы["T1RXXXXG7"];
			
			докЭкспорт.DOG_CPH 	= СтрокаГруппы["T1RXXXXG8"];
			
			докЭкспорт.NUMIDENT = СтрокаГруппы["T1RXXXXG9S"];
			докЭкспорт.LN		= СтрокаГруппы["T1RXXXXG101S"];
			докЭкспорт.NM		= СтрокаГруппы["T1RXXXXG102S"];
			докЭкспорт.FTN		= СтрокаГруппы["T1RXXXXG103S"];
			докЭкспорт.START_DT = ?(ЗначениеЗаполнено(СтрокаГруппы["T1RXXXXG111"]), День(СтрокаГруппы["T1RXXXXG111"]),"");
			докЭкспорт.END_DT   = ?(ЗначениеЗаполнено(СтрокаГруппы["T1RXXXXG112"]), День(СтрокаГруппы["T1RXXXXG112"]),"");
			
			докЭкспорт.NRM_DT	= ?(ЗначениеЗаполнено(СтрокаГруппы["T1RXXXXG12D"]), Формат(СтрокаГруппы["T1RXXXXG12D"], "ДФ=ddMMyyyy"),"");
			
			докЭкспорт.PID_ZV		= СтрокаГруппы["T1RXXXXG13S"];
			
			КоличествоСтрок = КоличествоСтрок + 1;

		КонецЦикла;
			
	ИначеЕсли ИмяСтраницы = "Таблица6" Тогда
		
		СтруктураГруппы = Неопределено;
		
		ДанныеФормы 	= ЭтаФорма[СтруктураРеквизитовФормы.мИдГруппы2];
		СтруктураГруппы = ДанныеФормыВЗначение(ДанныеФормы, Тип("ТаблицаЗначений"));
		ТекИдГруппы = СтруктураРеквизитовФормы.мИдГруппы2; 
	
		Если ТаблицаМногострочнойЧастиПуста(СтруктураГруппы, ТекИдГруппы) Тогда		
			ДокЭкспорт.ЗакрытьФайл();
			СтруктураПараметровЛиста.Вставить("ИмяФайла", ИмяФайла);
			Возврат;
		КонецЕсли;
		
		ИмяФайлаБезРасширения = "J30T406" + ВерсияВыгрузки;
		ИмяФайла              = ИмяФайлаБезРасширения+ ".DBF";
		
		ДокЭкспорт.поля.Добавить("PERIOD_M",		"N", 2);
		ДокЭкспорт.поля.Добавить("PERIOD_Y",		"N", 4);
		
		ДокЭкспорт.поля.Добавить("ROWNUM",		"N", 6);
		ДокЭкспорт.поля.Добавить("UKR_GROMAD",	"N", 1);
		ДокЭкспорт.поля.Добавить("ST",			"N", 1);
		
		ДокЭкспорт.поля.Добавить("NUMIDENT",	"C", 10);
		ДокЭкспорт.поля.Добавить("LN",			"C", 100);
		ДокЭкспорт.поля.Добавить("NM",			"C", 100);
		ДокЭкспорт.поля.Добавить("FTN",			"C", 100);
		ДокЭкспорт.поля.Добавить("ZO",			"N", 2);
		
		ДокЭкспорт.поля.Добавить("PAY_TP",		"N", 3);

		ДокЭкспорт.поля.Добавить("PAY_MNTH",	"N", 2);
		ДокЭкспорт.поля.Добавить("PAY_YEAR",	"N", 4);
		
		ДокЭкспорт.поля.Добавить("KD_NP",		"N", 3);
		ДокЭкспорт.поля.Добавить("KD_NZP",		"N", 3);
		
		ДокЭкспорт.поля.Добавить("KD_PTV",		"N", 3);
		ДокЭкспорт.поля.Добавить("KD_VP",		"N", 3);
		
		ДокЭкспорт.поля.Добавить("SUM_TOTAL",	"N", 16,2);
		ДокЭкспорт.поля.Добавить("SUM_MAX",		"N", 16,2);
		
		ДокЭкспорт.поля.Добавить("SUM_DIFF",	"N", 16,2);  // СуммаРазницы
		ДокЭкспорт.поля.Добавить("SUM_INS",		"N", 16,2);  // СуммаВзносовУдержано
		ДокЭкспорт.поля.Добавить("SUM_NARAH",	"N", 16,2);  // СуммаВзносовНачислено		
		ДокЭкспорт.поля.Добавить("OTK",			"N", 1);
		ДокЭкспорт.поля.Добавить("EXP",			"N", 1);
		
