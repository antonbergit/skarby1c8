// РАСЧЕТ()
// Выполняет расчет вычисляемых показателей на заданной странице отчета
// (ячеек, выделенных зеленым цветом). 
// В случае неуказания листа, ничего происходить не будет
// Параметры:
//  ИмяСтраницыПанели - строка, имя страницы основной панели формы.
//
&НаКлиентеНаСервереБезКонтекста
Процедура Расчет(Форма, ИмяСтраницыПанели = "", ОбластьИмя = "")
	
	Если (Форма.СтруктураРеквизитовФормы.мРежимПечати) Тогда
		Возврат; // служебный вызов в процессе печати
	КонецЕсли;
	
	Если (Форма.СтруктураРеквизитовФормы.ФлажокОтклАвтоРасчет) Тогда
		Возврат; // отключен режим авторасчета
	КонецЕсли;
	
	ЗаполнитьПоказательНаСервере("R01G3", ПолучитьПоказательНаСервере("R011G3", Форма, "Таблица1") + ПолучитьПоказательНаСервере("R012G3", Форма, "Таблица1")
	                                 + ПолучитьПоказательНаСервере("R013G3", Форма, "Таблица1") + ПолучитьПоказательНаСервере("R014G3", Форма, "Таблица1")
	                                 + ПолучитьПоказательНаСервере("R015G3", Форма, "Таблица1"), Форма, "Таблица1");
									 
	ЗаполнитьПоказательНаСервере("R02G3", ПолучитьПоказательНаСервере("R021G3", Форма, "Таблица1") + ПолучитьПоказательНаСервере("R022G3", Форма, "Таблица1")
	                                 + ПолучитьПоказательНаСервере("R023G3", Форма, "Таблица1") + ПолучитьПоказательНаСервере("R024G3", Форма, "Таблица1")
	                                 + ПолучитьПоказательНаСервере("R025G3", Форма, "Таблица1"), Форма, "Таблица1");
									 
	Ставка1  = РегламентированнаяОтчетностьКлиентСервер.ПолучитьСтавкуНалога(Форма.СтруктураРеквизитовФормы.мДатаНачалаПериодаОтчета);
	R021G3 = ПолучитьПоказательНаСервере("R021G3", Форма, "Таблица1");
	R031G3 = R021G3 * Ставка1/100;
	
	ЗаполнитьПоказательНаСервере("R031G3", R031G3, Форма, "Таблица1");
									 
	ЗаполнитьПоказательНаСервере("R03G3", ПолучитьПоказательНаСервере("R031G3", Форма, "Таблица1") + ПолучитьПоказательНаСервере("R032G3", Форма, "Таблица1")
	                                 + ПолучитьПоказательНаСервере("R033G3", Форма, "Таблица1") + ПолучитьПоказательНаСервере("R034G3", Форма, "Таблица1")
	                                 + ПолучитьПоказательНаСервере("R035G3", Форма, "Таблица1"), Форма, "Таблица1");
									 
