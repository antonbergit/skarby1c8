		Строка["T2RXXXXG19"]  = ?(МинЗП-ВыборкаТаблица6.База > 0,МинЗП-ВыборкаТаблица6.База,0);
		Строка["T2RXXXXG18"]  = ВыборкаТаблица6.База;
		Строка["T2RXXXXG111D"]  = СтруктураРеквизитовФормы.мДатаКонцаПериодаОтчета;
		Строка["T2RXXXXG111"]  = Месяц(СтруктураРеквизитовФормы.мДатаКонцаПериодаОтчета);
		Строка["T2RXXXXG112"]  = Год(СтруктураРеквизитовФормы.мДатаКонцаПериодаОтчета);
		Строка["T2RXXXXG17"]  = ВыборкаТаблица6.База;
		Строка["T2RXXXXG22"]  = 1;
		Строка["T2RXXXXG20"]  = ВыборкаТаблица6.ЕСВработник;
		Строка["T2RXXXXG21"]  = ВыборкаТаблица6.ЕСВФОТ;
		
		Номер = Номер + 1;
		
	КонецЦикла;
	
	ЗначениеВРеквизитФормы(СтруктураГруппы, СтруктураРеквизитовФормы.мИдГруппы2);
		
