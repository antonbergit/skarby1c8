						СтрокаТЧ.Флажок = Истина;
					КонецЦикла;
					ЭкспортПлатежей(КонПериода, Истина)
				КонецЕсли
			КонецЦикла;
			ТаблицаИДГруппыЧП.Загрузить(вТаблицаИДГруппыЧП.Выгрузить());
			Организация = вОрганизация;
			ТокенАК = вTokenАвтоклиента;
			ФлагАвтоИмпЭксп = Ложь;
			Если АвтоИмпорт Тогда
				ТекстСообщения = НСтр("ru='Выполнен автоматический импорт.';uk='Виконано автоматичний імпорт.'");
				СообщениеПользователю(ТекстСообщения, Ложь)
			КонецЕсли;
			Если АвтоЭкспорт Тогда
				ТекстСообщения = НСтр("ru='Выполнен автоматический мультиаккаунтный экспорт.';uk='Виконано автоматичний мультіаккаунтний експорт.'");
				СообщениеПользователю(ТекстСообщения, Ложь)
			КонецЕсли;
		КонецЕсли

	Иначе//Если Не ФлажокМультиАккаунтныйИмпорт Тогда
		//	обычный автоимпорт

		Если АвтоИмпорт Тогда
			
			ИмпортПлатежныеДокументы.Очистить();
			
			ИмпортПлатежей(НачалоДня(ТекущаяДата()), КонецДня(ТекущаяДата()));//НачПериода, КонПериода);
			
			Для Каждого СтрокаТЧ Из ИмпортПлатежныеДокументы Цикл
				СтрокаТЧ.Флажок = Истина;
			КонецЦикла;
			
			СоздатьИмпортированныеДокументы(НачПериода, КонПериода);
			
			ТекстСообщения = НСтр("ru='Выполнен автоматический импорт.';uk='Виконано автоматичний імпорт.'");
			СообщениеПользователю(ТекстСообщения, Ложь);
			
		КонецЕсли;
		
		Если АвтоЭкспорт Тогда
			
			ЭкспортПлатежныеДокументы.Очистить();
			
			ЗаполнитьСписокПлатежей(НачалоДня(ТекущаяДата()), КонецДня(ТекущаяДата()));//НачПериода, КонПериода);
			
			Для Каждого СтрокаТЧ Из ЭкспортПлатежныеДокументы Цикл
				СтрокаТЧ.Флажок = Истина;
			КонецЦикла;
			
			ЭкспортПлатежей(КонПериода, Истина);
			
			ТекстСообщения = НСтр("ru='Выполнен автоматический экспорт.';uk='Виконано автоматичний експорт.'");
			СообщениеПользователю(ТекстСообщения, Ложь);

		КонецЕсли

	КонецЕсли;

	Если (АвтоИмпорт Или АвтоЭкспорт) И (ФлажокМультиАккаунтныйИмпорт И ЕстьОтмечМультиАккАвтоОбмен Или Не ФлажокМультиАккаунтныйИмпорт)
			И Не ФлагАвтоИмпЭксп Тогда
		ТекстСообщения = НСтр("ru='Окончание автоматического обмена.';uk='Закінчення автоматичного обміну.'");
		СообщениеПользователю(ТекстСообщения, Ложь)
	КонецЕсли
	
КонецПроцедуры

Функция ПроверитьУчетнуюПолитикуПоОрганизации(Организация) Экспорт

	Если Найти("BASR,BSB,UNF,OSMD,UABY,UT_3", ИдКонф) > 0 Тогда//OSMD учетная политика не связана с искомыми параметрами организации
		Возврат Неопределено
	КонецЕсли;

	ТекДата = ТекущаяДата();
	ЕстьЕдиныйНалог = Ложь;
	Попытка
		ИмяРСУчетнойПолитики = "УчетнаяПолитикаОрганизаций";
		УчетнаяПолитикаОрганизации = РегистрыСведений[ИмяРСУчетнойПолитики].ПолучитьПоследнее(ТекДата, Новый Структура("Организация", Организация));
		Если ИдКонф = "ERP" Тогда
			// ERP 2.1 (BASIEM2.1)
			УчетнаяПолитикаПустая = УчетнаяПолитикаОрганизации.УчетнаяПолитика = Справочники.УчетныеПолитикиОрганизаций.ПустаяСсылка();
			Если Не УчетнаяПолитикаПустая Тогда
				ЕстьЕдиныйНалог = УчетнаяПолитикаОрганизации.ПлательщикЕН
				//альтернативный вариант - не используется
				//УчетнаяПолитикаПустая = УчетнаяПолитикаОрганизации.УчетнаяПолитика.СистемаНалогообложения = Справочники.УчетныеПолитикиОрганизаций.СистемаНалогообложения.ЕдиныйНалог
				//	Или УчетнаяПолитикаОрганизации.УчетнаяПолитика.СистемаНалогообложения = Справочники.УчетныеПолитикиОрганизаций.СистемаНалогообложения.ЕдиныйНалогИНДС
			КонецЕсли
		Иначе
			// BASC, BP, ... 
			УчетнаяПолитикаПустая = УчетнаяПолитикаОрганизации.СхемаНалогообложения = Справочники.СхемыНалогообложения.ПустаяСсылка();
			Если Не УчетнаяПолитикаПустая Тогда
				ЕстьЕдиныйНалог = УчетнаяПолитикаОрганизации.СхемаНалогообложения = Справочники.СхемыНалогообложения.ЕдиныйНалог Или
					УчетнаяПолитикаОрганизации.СхемаНалогообложения = Справочники.СхемыНалогообложения.ЕдиныйНалогИНДС
			КонецЕсли
		КонецЕсли
	Исключение
		Попытка
			// AAA
			ИмяРСУчетнойПолитики = "УчетныеПолитики";
			УчетнаяПолитикаОрганизации = РегистрыСведений[ИмяРСУчетнойПолитики].ПолучитьПоследнее(ТекДата, Новый Структура("Объект", Организация));
			УчетнаяПолитикаПустая = Не ЗначениеЗаполнено(УчетнаяПолитикаОрганизации.Значение);
			Если Не УчетнаяПолитикаПустая Тогда
				ЕстьЕдиныйНалог = УчетнаяПолитикаОрганизации.Значение = ПланыВидовХарактеристик.УчетныеПолитики.ВидНалога.ПерваяГруппа Или
					ПланыВидовХарактеристик.УчетныеПолитики.ВидНалога.ВтораяГруппа Или
					ПланыВидовХарактеристик.УчетныеПолитики.ВидНалога.ТретьяГруппа
			КонецЕсли
		Исключение
			Попытка
				// ERP 2.5 (BASIEM2.5)
				ИмяРСУчетнойПолитики = "НастройкиСистемыНалогообложения";
				УчетнаяПолитикаОрганизации = РегистрыСведений[ИмяРСУчетнойПолитики].ПолучитьПоследнее(ТекДата, Новый Структура("Организация", Организация));
				УчетнаяПолитикаПустая = УчетнаяПолитикаОрганизации.СистемаНалогообложения = Перечисления.СистемыНалогообложения.ПустаяСсылка();
				ЕстьЕдиныйНалог = УчетнаяПолитикаОрганизации.СистемаНалогообложения = Перечисления.СистемыНалогообложения.ЕдиныйНалог
					Или УчетнаяПолитикаОрганизации.СистемаНалогообложения = Перечисления.СистемыНалогообложения.ЕдиныйНалогИНДС;
			Исключение
				ТекстСообщения = НСтр("ru='Ошибка при получении учетной политики по организации ';uk='Помилка при отриманні облікової політики по організації '")
					+ Организация.Наименование + Символы.ПС + ОписаниеОшибки();
				СообщениеПользователю(ТекстСообщения, Ложь);
				Возврат Неопределено
			КонецПопытки
		КонецПопытки
	КонецПопытки;

	СинонимУчетнойПолитики = Метаданные["РегистрыСведений"][ИмяРСУчетнойПолитики].Синоним;
	Если УчетнаяПолитикаПустая Тогда
		ТекстСообщения = НСтр("ru='Отсутствует учетная политика (';uk='Відсутня облікова політика ('") + СинонимУчетнойПолитики
			+ НСтр("ru=') по организации ';uk=') по організації '") + Организация + НСтр("ru=' на дату ';uk=' на дату '") + ТекДата;
		СообщениеПользователю(ТекстСообщения, Ложь);
		Возврат Неопределено
	КонецЕсли;

	Возврат ЕстьЕдиныйНалог

КонецФункции

Процедура АвтоОбменДокументы(НачПериода, КонПериода, спИмяПечатнойФормы, ВидДокументаPDFСписокВыбора) Экспорт

	ЕстьОтмечМультиАккАвтоОбменДокументы = Ложь;
	Для каждого СтрокаТокеныКомпаний Из ТокеныКомпаний Цикл
		Если СтрокаТокеныКомпаний.КолонкаОтметкаДокументы Тогда
			ЕстьОтмечМультиАккАвтоОбменДокументы = Истина;
			Прервать
		КонецЕсли
	КонецЦикла;

	Если (ФлажокАвтоИмпортДокументы Или ФлажокАвтоОтправкаДокументы) И (ФлажокМультиАккаунтныйОбменДокументы И ЕстьОтмечМультиАккАвтоОбменДокументы
			Или Не ФлажокМультиАккаунтныйОбменДокументы) И Не ФлагАвтоИмпЭкспДокументы Тогда
		ТекстСообщения = НСтр("ru='Начало сессии автоматического обмена документами.';uk='Початок сесії автоматичного обміну документами.'");
		СообщениеПользователю(ТекстСообщения, Ложь)
	КонецЕсли;	

	Если ФлагАвтоИмпЭкспДокументы Тогда
		Возврат
	КонецЕсли;

	вНачПериода = НачПериода;
	вКонПериода = КонПериода;
	// на случай круглосуточной работы АО
	НачПериода = НачалоДня(ТекущаяДата());
	КонПериода = КонецДня(ТекущаяДата());

	ПереченьДок = "";

	Если ФлажокМультиАккаунтныйОбменДокументы Тогда
		Если Не ЕстьОтмечМультиАккАвтоОбменДокументы Тогда
			Возврат
		КонецЕсли;

		//	Мультиаккаунтный автоимпорт
		Если (ФлажокАвтоИмпортДокументы Или ФлажокАвтоОтправкаДокументы) И Не ФлагАвтоИмпЭкспДокументы Тогда
			ФлагАвтоИмпЭкспДокументы = Истина;

			//сохранение текущей организации
			вКодКомпании = КодПоЕДРПОУ;
			вTokenАвтоклиента = ТокенАК;

			Для каждого СтрокаТЗБА Из ТокеныКомпаний Цикл
				Если Не СтрокаТЗБА.КолонкаОтметкаДокументы Тогда
					Продолжить
				КонецЕсли;

				Организация = НайтиОрганизацию(СтрокаТЗБА.КодКомпании);
				Если Организация = Справочники.Организации.ПустаяСсылка() Тогда
					Продолжить
				КонецЕсли;
				ЕстьЕдиныйНалог = ПроверитьУчетнуюПолитикуПоОрганизации(Организация);
				ЗаполнитьСведенияОбОрганизации(Организация, ТекущаяДата(), ЕстьЕдиныйНалог);
				ТокенАК = СтрокаТЗБА.ТокенАвтоклиента;

				ТекстСообщения = НСтр("ru='Автообмен документами по организации ';uk='Автообмін документами по організації '") + Организация.Наименование;//+ " токен " + ТокенАК;
				СообщениеПользователю(ТекстСообщения, Ложь);
				Если ФлажокАвтоИмпортДокументы Тогда
					вТекВидОперации = ТекВидОперации;
					АвтоИмпортНалоговыеЗапросЕРННФинДок(НачПериода, КонПериода, ПереченьДок);
					ТекВидОперации = вТекВидОперации
				КонецЕсли;
				Если ФлажокАвтоОтправкаДокументы Тогда
					вТекВидОперации = ТекВидОперации;
					АвтоЭкспортНалоговыеЗапросЕРННФинДок(НачПериода, КонПериода, ПереченьДок, спИмяПечатнойФормы, ВидДокументаPDFСписокВыбора);
					ТекВидОперации = вТекВидОперации
				КонецЕсли
			КонецЦикла;

			//восстановление текущей организации
			Организация = НайтиОрганизацию(вКодКомпании);
			Если Организация <> Справочники.Организации.ПустаяСсылка() Тогда
				ЕстьЕдиныйНалог = ПроверитьУчетнуюПолитикуПоОрганизации(Организация);
				ЗаполнитьСведенияОбОрганизации(Организация, ТекущаяДата(), ЕстьЕдиныйНалог)
			КонецЕсли;
			ТокенАК = вTokenАвтоклиента;

			ФлагАвтоИмпЭкспДокументы = Ложь;
			Если ФлажокАвтоИмпортДокументы Тогда
				ТекстСообщения = НСтр("ru='Выполнен автоматический мультиаккаунтный импорт документов.';uk='Виконано автоматичний мультіаккаунтний імпорт документів.'");
				СообщениеПользователю(ТекстСообщения, Ложь)
			КонецЕсли;
			Если ФлажокАвтоОтправкаДокументы Тогда
				ТекстСообщения = НСтр("ru='Выполнен автоматический мультиаккаунтный экспорт документов.';uk='Виконано автоматичний мультіаккаунтний експорт документів.'");
				СообщениеПользователю(ТекстСообщения, Ложь)
			КонецЕсли;
		КонецЕсли

	Иначе//Если Не ФлажокМультиАккаунтныйИмпорт Тогда
		//	обычный автоимпорт

		Если ФлажокАвтоИмпортДокументы И Не ФлагАвтоИмпЭкспДокументы Тогда
			ФлагАвтоИмпЭкспДокументы = Истина;
			ПереченьДок = Строка(ТекущаяДата()) + "Импорт: ";
			вТекВидОперации = ТекВидОперации;
			АвтоИмпортНалоговыеЗапросЕРННФинДок(НачПериода, КонПериода, ПереченьДок);
			ТекВидОперации = вТекВидОперации;
			ТекстСообщения = НСтр("ru='Выполнен автоматический импорт.';uk='Виконано автоматичний імпорт.'");
			СообщениеПользователю(ТекстСообщения, Ложь);
			ФлагАвтоИмпЭкспДокументы = Ложь
		КонецЕсли;
		
		Если ФлажокАвтоОтправкаДокументы И Не ФлагАвтоИмпЭкспДокументы Тогда
			ФлагАвтоИмпЭкспДокументы = Истина;
			ПереченьДок = Строка(ТекущаяДата()) + "Экспорт: ";
			вТекВидОперации = ТекВидОперации;
			АвтоЭкспортНалоговыеЗапросЕРННФинДок(НачПериода, КонПериода, ПереченьДок, спИмяПечатнойФормы, ВидДокументаPDFСписокВыбора);
			ТекВидОперации = вТекВидОперации;
			ТекстСообщения = НСтр("ru='Выполнен автоматический экспорт документов.';uk='Виконано автоматичний експорт документів.'");
			СообщениеПользователю(ТекстСообщения, Ложь);
			ФлагАвтоИмпЭкспДокументы = Ложь
		КонецЕсли

	КонецЕсли;

	Если (ФлажокАвтоИмпортДокументы Или ФлажокАвтоОтправкаДокументы) И (ФлажокМультиАккаунтныйОбменДокументы И ЕстьОтмечМультиАккАвтоОбменДокументы
			Или Не ФлажокМультиАккаунтныйОбменДокументы) И Не ФлагАвтоИмпЭкспДокументы Тогда
		ТекстСообщения = НСтр("ru='Окончание сессии автоматического обмена документов.';uk='Закінчення сесії автоматичного обміну документів.'");
		СообщениеПользователю(ТекстСообщения, Ложь)
	КонецЕсли;

	НачПериода = вНачПериода;
	КонПериода = вКонПериода

КонецПроцедуры

Процедура АвтоИмпортНалоговыеЗапросЕРННФинДок(НачПериода, КонПериода, ПереченьДок)

	//локЕстьОшибки = Ложь;//пока не используется, может в будущем

	// импорт (в т.ч. мультиакк) налоговых
	Если (ФлажокАОНН Или ФлажокАОП2кНН) И ИдКонф <> "BASR" И ИдКонф <> "AAA" Тогда

		ТекВидОперации = "ВходящиеНН";
		СписокДокументов.Очистить();
		ВыпВЦикле = Истина;
		Пока ВыпВЦикле Цикл
			ВыпВЦикле = ЗагрузитьВходящиеНН(НачПериода, КонПериода, "РегистрацияВходящейНН");
		КонецЦикла;

		// отметка нужного(ых) док
		ЕстьОтмеченныеДок = Ложь;
		Для каждого СтрокаСпискаДок Из СписокДокументов Цикл
			Если ФлажокАОНН И СтрокаСпискаДок.ВидОперации = "Налоговая"
					Или ФлажокАОП2кНН И СтрокаСпискаДок.ВидОперации = "Приложение" Тогда
				СтрокаСпискаДок.Флажок = Истина;
				ЕстьОтмеченныеДок = Истина
			КонецЕсли
		КонецЦикла;
		Если ЕстьОтмеченныеДок Тогда

			ТекстДокАО = "";
			Если ФлажокАОНН Тогда
				ТекстДокАО = ТекстДокАО + НСтр("ru='налоговая накладная';uk='податкова накладна'")
			КонецЕсли;	
			Если ФлажокАОП2кНН Тогда
				ТекстДокАО = ТекстДокАО + ?(ЗначениеЗаполнено(ТекстДокАО), НСтр("ru=' и ';uk=' і '"), "")
					+ НСтр("ru='приложение к налоговой накладной';uk='додаток до податкової накладної'")
			КонецЕсли;
			ТекстДокАО = НСтр("ru='Импорт документов ';uk='Імпорт документів '") + ТекстДокАО + ".";
			СообщениеПользователю(ТекстДокАО, Ложь);

			// запись док
			ВыпВЦикле = Истина;
			Пока ВыпВЦикле Цикл
				ВыпВЦикле = ЗаписатьВходящиеНН("РегистрацияВходящегоНалоговогоДокумента")
			КонецЦикла;

			Если ФлажокАОНН Тогда
				ПереченьДок = ПереченьДок + ?(ЗначениеЗаполнено(ПереченьДок) И Прав(ПереченьДок, 6) <> "порт: ", ", ", "")
					+ НСтр("ru='налоговые накладные';uk='податкові накладні'")
			КонецЕсли;	
			Если ФлажокАОП2кНН Тогда//Или ФлажокАОНН Тогда
				ПереченьДок = ПереченьДок + ?(ЗначениеЗаполнено(ПереченьДок) И Прав(ПереченьДок, 6) <> "порт: ", ", ", "")
					+ НСтр("ru='приложение 2 к налоговым накладным';uk='додаток 2 до податкових накладних'")
			КонецЕсли

		//Иначе
		//	// сообщение об отсутствии док выводится в модуле
		КонецЕсли;
		//локЕстьОшибки = ЕстьОшибки//пока не используется, может в будущем
	КонецЕсли;


	//// импорт (в т.ч. мультиакк) Запрос к ЕРНН
	//Если ФлажокЗапросКЕРНН И Найти("BASR,AAA,UABY", ИдКонф) = 0 Тогда
	//	ТекВидОперации = "Налоговые";//"Запрос к ЕРНН";
	//	СписокДокументов.Очистить();
	//	ВыпВЦикле = Истина;
	//	Пока ВыпВЦикле Цикл
	//		ВыпВЦикле = ЗагрузитьВходящиеНН(НачПериода, КонПериода, "РегистрацияВходящейНН");
	//	КонецЦикла;
	//	// отметка док
	//	ЕстьОтмеченныеДок = Ложь;
	//	Для каждого СтрокаСпискаДок Из СписокДокументов Цикл
	//		СтрокаСпискаДок.Флажок = Истина;
	//		ЕстьОтмеченныеДок = Истина
	//	КонецЦикла;
	//	Если ЕстьОтмеченныеДок Тогда
	//		ТекстДокАО = НСтр("ru='Импорт документов Запрос к ЕРНН.';uk='Імпорт документів Запит до ЄРПН'");
	//		СообщениеПользователю(ТекстДокАО, Ложь);
	//		// запись док
	//		ВыпВЦикле = Истина;
	//		Пока ВыпВЦикле Цикл
	//			ВыпВЦикле = ЗаписатьВходящиеНН(ТекВидОперации)
	//		КонецЦикла;
	//		ПереченьДок = ПереченьДок + ?(ЗначениеЗаполнено(ПереченьДок) И Прав(ПереченьДок, 6) <> "порт: ", ", ", "")
	//			+ НСтр("ru='запрос к ЕРНН';uk='запит до ЄРПН'")
	//	Иначе
	//		// сообщение об отсутствии док выводится в модуле
	//	КонецЕсли;
	//	локЕстьОшибки = локЕстьОшибки Или ЕстьОшибки//пока не используется, может в будущем
	//КонецЕсли;


	// импорт (в т.ч. мультиакк) финдок
	Если ФлажокАОСчета Или ФлажокАОЗаказы Или ФлажокАОАкты Или ФлажокАОНакладные Тогда

		ФинансовыеДокументы.Очистить();

		сзВыбранныеФинДок = Новый СписокЗначений;
		Если ФлажокАОСчета Тогда
			сзВыбранныеФинДок.Добавить("Счета", НСтр("ru='счета на оплату';uk='рахунки на оплату'"))
		КонецЕсли;
		Если ФлажокАОЗаказы И Найти("BP,BASC,UABY", ИдКонф) = 0 Тогда
			сзВыбранныеФинДок.Добавить("Заказы", НСтр("ru='заказы';uk='замовлення'"))
		КонецЕсли;
		Если ФлажокАОАкты И ИдКонф <> "AAA" Тогда
			сзВыбранныеФинДок.Добавить("Акты", НСтр("ru='акты выполненных работ';uk='акти виконаних работ'"))
		КонецЕсли;
		Если ФлажокАОНакладные Тогда
			сзВыбранныеФинДок.Добавить("Расходные", НСтр("ru='расходные накладные';uk='видаткові накладні'"))
		КонецЕсли;

		Для каждого элсзВыбранныеФинДок Из сзВыбранныеФинДок Цикл

			ТекВидОперации = элсзВыбранныеФинДок.Значение;

			// импорт док
			showOnlyUnread = Ложь;
			ВыпВЦикле = Истина; 
			Пока ВыпВЦикле Цикл
				ВыпВЦикле = ЗагрузитьФинДок(НачПериода, КонПериода, ТекВидОперации, showOnlyUnread)
			КонецЦикла;

			// отметка док
			ЕстьОтмеченныеДок = Ложь;
			Для каждого СтрокаСпискаДок Из ФинансовыеДокументы Цикл
				СтрокаСпискаДок.Флажок = Истина;
				ЕстьОтмеченныеДок = Истина;
			КонецЦикла;
			Если Не ЕстьОтмеченныеДок Тогда
				// сообщение об отсутствии док выводится в модуле
				Продолжить
			КонецЕсли;

			ТекстДокАО = НСтр("ru='Импорт документов ';uk='Імпорт документів '") + элсзВыбранныеФинДок.Представление;
			СообщениеПользователю(ТекстДокАО, Ложь);

			// запись док
			ВыпВЦикле = Истина; 
			Пока ВыпВЦикле Цикл
				ВыпВЦикле = ЗаписатьВходящиеФинДок(ТекВидОперации)
			КонецЦикла;

			ПереченьДок = ПереченьДок + ?(ЗначениеЗаполнено(ПереченьДок) И Прав(ПереченьДок, 6) <> "порт: ", ", ", "") + элсзВыбранныеФинДок.Представление

		КонецЦикла;
		//локЕстьОшибки = локЕстьОшибки Или ЕстьОшибки//пока не используется, может в будущем
	КонецЕсли;

	//пока не используется, может в будущем
