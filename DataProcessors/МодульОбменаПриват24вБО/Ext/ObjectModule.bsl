Процедура АвтоЭкспортНалоговыеЗапросЕРННФинДок(НачПериода, КонПериода, ПереченьДок, спИмяПечатнойФормы, ВидДокументаPDFСписокВыбора)

	//локЕстьОшибки = Ложь;//пока не используется, может в будущем

	// автоотправка (в т.ч. мультиакк) налоговых док и П2
	Если (ФлажокАОНН Или ФлажокАОП2кНН Или ФлажокЗапросКЕРНН) И ИдКонф <> "BASR" И ИдКонф <> "AAA" Тогда

		ТекВидОперации = "Налоговые";
		СписокДокументов.Очистить();

		сзВыбранныеНалоговые = Новый СписокЗначений;
		Если ФлажокАОНН Тогда
			сзВыбранныеНалоговые.Добавить("НН", НСтр("ru='налоговая накладная';uk='податкова накладна'"))
		КонецЕсли;
		Если ФлажокАОП2кНН Тогда
			сзВыбранныеНалоговые.Добавить("П2", НСтр("ru='приложение 2 к налоговой накладной';uk='додаток 2 до податкової накладної'"))
		КонецЕсли;
		Если ФлажокЗапросКЕРНН Тогда
			сзВыбранныеНалоговые.Добавить("ЗЕ", НСтр("ru='запрос к общему реестру налоговых накладных ГНАУ';uk='запит до загального реєстру податкових накладних ДПАУ'"))
		КонецЕсли;

		Для каждого эсзсзВыбранныеНалоговые Из сзВыбранныеНалоговые Цикл

			// заполнение док
			Отборы = Новый Структура;
			ОтборПоЖурналу = Новый СписокЗначений;
			ОтборПоЖурналу.Добавить("К выгрузке");
			ОтборПоВидуДок = Новый СписокЗначений;
			ОтборПоВидуДок.Добавить("Все виды документов");
			Отборы.Вставить("ОтборПоЖурналу", ОтборПоЖурналу);
			Отборы.Вставить("ОтборПоВидуДок", ОтборПоВидуДок);
			ЗаполнитьСписок(Отборы, НачПериода, КонПериода);

			// отметка нужного(ых) док
			ЕстьОтмеченныеДок = Ложь;
			Для каждого СтрокаСпискаДок Из СписокДокументов Цикл
				Если ФлажокАОНН И ФлажокАОП2кНН И ФлажокЗапросКЕРНН
						Или ФлажокАОНН И эсзсзВыбранныеНалоговые.Значение = "НН" И СтрокаСпискаДок.ВидДокумента = "Налоговая накладная"
						Или ФлажокАОП2кНН И эсзсзВыбранныеНалоговые.Значение = "П2" И СтрокаСпискаДок.ВидДокумента = "Приложение 2 к налоговой накладной"
						Или ФлажокЗапросКЕРНН И эсзсзВыбранныеНалоговые.Значение = "ЗЕ" И СтрокаСпискаДок.ВидДокумента = "Запит до ЄРПН" Тогда
					СтрокаСпискаДок.Флажок = Истина;
					ЕстьОтмеченныеДок = Истина
				КонецЕсли
			КонецЦикла;
			Если Не ЕстьОтмеченныеДок Тогда
				ТекстСообщения = НСтр("ru='Отсутствуют документы %документ% для отправки.';uk='Відсутні документи %документ% для відправки.'");
				ТекстСообщения = СтрЗаменить(ТекстСообщения, "%документ%", эсзсзВыбранныеНалоговые.Представление);
				СообщениеПользователю(ТекстСообщения);
				Продолжить
			КонецЕсли;

			ТекстДокАО = "";
			Если ФлажокАОНН Тогда
				ТекстДокАО = ТекстДокАО + НСтр("ru='налоговая накладная';uk='податкова накладна'")
			КонецЕсли;	
			Если ФлажокАОП2кНН Тогда
				ТекстДокАО = ТекстДокАО + ?(ЗначениеЗаполнено(ТекстДокАО), НСтр("ru=' и ';uk=' і '"), "")
					+ НСтр("ru='приложение к налоговой накладной';uk='додаток до податкової накладної'")
			КонецЕсли;
			Если ФлажокЗапросКЕРНН Тогда
				ТекстДокАО = ТекстДокАО + ?(ЗначениеЗаполнено(ТекстДокАО), НСтр("ru=' и ';uk=' і '"), "")
					+ НСтр("ru='запрос к общему реестру налоговых накладных ГНАУ';uk='запит до загального реєстру податкових накладних ДПАУ'")
			КонецЕсли;
			ТекстДокАО = НСтр("ru='Отправка документов ';uk='Відправка документів '") + ТекстДокАО + ".";
			СообщениеПользователю(ТекстДокАО, Ложь);

			// отправка док
			ПроверитьНаличиеСтатусовПолучитьСтатусы(ТекВидОперации);
			ВыпВЦикле = Истина;
			Пока ВыпВЦикле Цикл
				ВыпВЦикле = СформироватьXML_Налоговые(КонПериода, Истина);//параметр Истина - передача на сервис Приват24
			КонецЦикла;

			Если эсзсзВыбранныеНалоговые.Значение = "НН" Тогда
				ПереченьДок = ПереченьДок + ?(ЗначениеЗаполнено(ПереченьДок) И Прав(ПереченьДок, 6) <> "порт: ", ", ", "")
					+ НСтр("ru='налоговые накладные';uk='податкові накладні'")
			КонецЕсли;	
			Если эсзсзВыбранныеНалоговые.Значение = "П2" Или ФлажокАОНН Тогда
				ПереченьДок = ПереченьДок + ?(ЗначениеЗаполнено(ПереченьДок) И Прав(ПереченьДок, 6) <> "порт: ", ", ", "")
					+ НСтр("ru='приложение 2 к налоговым накладным';uk='додаток 2 до податкових накладних'")
			КонецЕсли;
