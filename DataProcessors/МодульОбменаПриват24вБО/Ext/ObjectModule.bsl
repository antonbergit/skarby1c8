	СтрокаТЗ.R01G9 = ИтСуммаG9;
	СтрокаТЗ.R02G111 = ИтСуммаНДС7; //итог сумма НДС по основой ставке НДС 7%
	СтрокаТЗ.R03G14 = ИтСуммаНДС14; //итог сумма НДС по основой ставке НДС 14%
	
	СтрокаТЗ.R006G03 = ИтСуммаR006G03; 
