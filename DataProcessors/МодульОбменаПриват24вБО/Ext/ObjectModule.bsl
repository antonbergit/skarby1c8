			НовыйДок = Документы[ИмяДокумента].СоздатьДокумент();
			НовыйДок.Заполнить(СтрокаТЧ.СсылкаНаДокумент);
			НовыйДок.Дата = ТекущаяДата();
			НовыйДок.ПроверитьЗаполнение();
			//дополнительно проверить (и заполнить по умолчанию) банковские счета орг/контр
			НовыйДок.Записать();
			ТекстСообщения = НСтр("ru = 'Создан и записан платежній документ %ПлатДок% на основании счета в строке ';uk='Створений і записаний платіжний документ %ПлатДок% на підставі рахунку в рядку '")
				+ СтрокаТЧ.НомерСтроки;
			ТекстСообщения = СтрЗаменить(ТекстСообщения, "%ПлатДок%", Строка(НовыйДок));
			СообщениеПользователю(ТекстСообщения + ОписаниеОшибки(), Ложь);
			КолСозданоПлатежейИсх = КолСозданоПлатежейИсх + 1
		Исключение
			ТекстСообщения = НСтр("ru = 'Ошибка при создании/записи платежного документа на основании в строке ';uk='Помилка при створенні/запису платіжного документа на підставі в рядку '")
				+ СтрокаТЧ.НомерСтроки + " : ";
			СообщениеПользователю(ТекстСообщения + ОписаниеОшибки());
			КолОшибокПриСозданииПлатежейИсх = КолОшибокПриСозданииПлатежейИсх + 1
		КонецПопытки
	КонецЦикла;

	Если КолСозданоПлатежейИсх > 0 Тогда
		ТекстСообщения = НСтр("ru = 'Создано %КолСозданоПлатежейИсх% платежных документов на основании счетов на оплату.';uk='Створено %КолСозданоПлатежейИсх% платіжних документів на підставі рахунків на оплату.'");
		ТекстСообщения = СтрЗаменить(ТекстСообщения, "%КолСозданоПлатежейИсх%", КолСозданоПлатежейИсх);
