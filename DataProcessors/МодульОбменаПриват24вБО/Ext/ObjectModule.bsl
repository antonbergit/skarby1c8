			КонецЕсли;
			СообщениеПользователю(ТекстСообщения, Ложь);
			Продолжить
		КонецЕсли;

		РесурсНаСервере = "/api/proxy/edoc/journal/sign-info/" + IdДок + "?okpo=" + ЕДРПОУ;
