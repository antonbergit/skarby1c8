		Иначе
			Результат = СокрЛП(Значение);
		КонецЕсли;			
		
		Попытка
			Контроль = Число(Результат);
			Если  (ВРег(Тип) = ВРег("DGKod0")) И (Результат = "0") Тогда
				// это допустимо
			ИначеЕсли  (ВРег(Тип) = ВРег("Kod0Column")) И (Результат = "0") Тогда
