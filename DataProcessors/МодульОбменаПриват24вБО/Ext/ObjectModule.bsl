					НоваяСтрокаКорпорация.НомерКарты = НомерКарты;
					НомерКарточногоСчета = эмКорпорации.MainCardAcc;
					НоваяСтрокаКорпорация.НомерКарточногоСчета = ?(ЗначениеЗаполнено(НомерКарточногоСчета), НомерКарточногоСчета, НомерКарты);
