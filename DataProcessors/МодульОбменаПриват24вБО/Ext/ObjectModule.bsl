Функция ПолучитьМассивБДДИзHexСтроки(Знач пСтрока)

	лПоз = Найти(пСтрока,",");
	лМассив = Новый Массив();
	Пока лПоз>1 Цикл
		лПодстрока = Сред(пСтрока,1,лПоз-1);
		пСтрока = Сред(пСтрока,лПоз+1);
