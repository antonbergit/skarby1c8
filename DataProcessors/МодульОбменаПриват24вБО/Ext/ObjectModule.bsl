					//НомерКарты = эмКорпорации.MainCardAcc//номер карточного счета
					НоваяСтрокаКорпорация.КолонкаТипКарты = ПарситьТипКарты(corpType)
				Исключение
					//	соответствие
					НоваяСтрокаКорпорация.branch = эмКорпорации["Branch"];
					НоваяСтрокаКорпорация.corporation = эмКорпорации["CorpCode"];
					НоваяСтрокаКорпорация.corpType = corpType;//эмКорпорации["corpType"];
					НоваяСтрокаКорпорация.КолонкаКорпорация = эмКорпорации["NameCard"];
					НоваяСтрокаКорпорация.КолонкаГруппа = 1;//группа
					ПозицияРавно = Найти(эмКорпорации["MainCardNum"], "=");
					НомерКарты = Лев(эмКорпорации["MainCardNum"], ПозицияРавно - 1);
					//НоваяСтрокаКорпорация.MainCardNum = эмКорпорации["MainCardNum"];
					НоваяСтрокаКорпорация.НомерКарты = НомерКарты;
					НомерКарточногоСчета = эмКорпорации["MainCardAcc"];
					НоваяСтрокаКорпорация.НомерКарточногоСчета = ?(ЗначениеЗаполнено(НомерКарточногоСчета), НомерКарточногоСчета, НомерКарты);
					//НоваяСтрокаКорпорация.НомерКарточногоСчета = эмКорпорации["MainCardAcc"];
					//НомерКарты = эмКорпорации["MainCardAcc"]//номер карточного счета
					НоваяСтрокаКорпорация.КолонкаТипКарты = ПарситьТипКарты(corpType)
				КонецПопытки
			КонецЕсли;
			Попытка
				//	структура
				эмКорпорацииPanData = эмКорпорации.PanData
			Исключение
				//	соответствие
				эмКорпорацииPanData = эмКорпорации["PanData"]
			КонецПопытки;
			Для каждого эмКарты Из эмКорпорацииPanData Цикл
				Попытка
					//	структура
					ПозицияРавно = Найти(эмКарты.PanNumber, "=");
					НомерКарты = Лев(эмКарты.PanNumber, ПозицияРавно - 1);
				Исключение
					//	соответствие
					ПозицияРавно = Найти(эмКарты["PanNumber"], "=");
					НомерКарты = Лев(эмКарты["PanNumber"], ПозицияРавно - 1);
				КонецПопытки;
				Если Найти("ПолучениеДанныхПоКорпорациямИПоКартамАК,ПолучениеДанныхПоВсемКартамАК,ПолучениеДанныхСкрытьЗакрытыеИПросроченныеКартыАК", ДействиеАналитики) > 0 Тогда
					//НоваяСтрокаКарта = ТабличноеПолеКорпКарты.Добавить();
					НоваяСтрокаКарта = тзКорпКарты.Добавить();
				ИначеЕсли ДействиеАналитики = "ВыпискаПоКартам" Тогда	
				Иначе//Если ДействиеАналитики = "ОтчетПоКартам" Тогда
					Если Не ЗначениеЗаполнено(НомерКарты) Тогда
						Продолжить
					КонецЕсли;
					НоваяСтрокаОтчет = ТабличноеПолеОтчет.Добавить()
				КонецЕсли;
				Если Найти("ПолучениеДанныхПоКорпорациямИПоКартамАК,ПолучениеДанныхПоВсемКартамАК,ПолучениеДанныхСкрытьЗакрытыеИПросроченныеКартыАК", ДействиеАналитики) > 0 Тогда
					Попытка
						//	структура
						НоваяСтрокаКарта.branch = эмКарты.Branch;//эмКорпорации.Branch;
