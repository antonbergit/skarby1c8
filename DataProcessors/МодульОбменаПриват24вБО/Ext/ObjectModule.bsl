		+ ЭкранироватьJSON(СтрокаЭкспорта.struct_category, УдалитьКавычки), "") + """";
	Если ЭтоЮрЛицоКонтрагент = Ложь И УсловиеВерующих Тогда
		Если ЗначениеЗаполнено(СтрокаЭкспорта.IDКарты) Тогда
			СтрокаЗапроса = СтрокаЗапроса + ", ""recipient_document_id_number"":""" + СтрокаЭкспорта.IDКарты + """"//ID-паспорт
		Иначе
			СтрокаЗапроса = СтрокаЗапроса + ", ""recipient_document_number"":""" + СтрокаЭкспорта.НомерПаспорта//номер паспорту
				+ """, ""recipient_document_series"":""" + СтрокаЭкспорта.СерияПаспорта + """"//серія паспорту
		КонецЕсли
	КонецЕсли;
	СтрокаЗапроса = СтрокаЗапроса + ", ""document_type"":""cr"", ""payment_date"":""" + СтрокаЭкспорта.ДатаПлатежа
		+ """, ""payment_accept_date"":""" + СтрокаЭкспорта.ДатаПлатежа + """}";

	//2024.01.29
	СтрокаЗапроса = СтрЗаменить(СтрЗаменить(СтрЗаменить(СтрокаЗапроса, Символ(182), ""), Символ(9), ""), Символы.Таб, "");//	КодСимвола("	")
	СтрокаЗапроса = СтрЗаменить(СтрЗаменить(СтрокаЗапроса, Символы.ПС, ""), Символы.ВК, "");
	////СтрокаЗапроса = СтрЗаменить(СтрокаЗапроса, Символ(32), "");//	убрать пробелы
	СтрокаЗапроса = СтрЗаменить(СтрЗаменить(СтрЗаменить(СтрокаЗапроса, Символы.ПФ, ""), Символы.ВТаб, ""), Символы.НПП, "");
	////СтрокаЗапроса = СтрЗаменить(СтрокаЗапроса, " ", "\ ");//	экранировать пробел
	////СтрокаЗапроса = СтрЗаменить(СтрЗаменить(СтрокаЗапроса, "'", "\'"), """", "\""")	//	экранировать кавычки
	//СтрокаЗапроса = СтрЗаменить(СтрокаЗапроса, "\", "\\");
	//end 2024.01.29
	ТелоЗапросаКурл = ЭкранироватьДвойныеКавычки(СтрокаЗапроса);
	ТелоЗапросаКурл = """" + ТелоЗапросаКурл + """";
	//2024.01.29
	//ТелоЗапросаКурл = СтрЗаменить(ТелоЗапросаКурл, Символы.ПС, "");
	//ТелоЗапросаКурл = СтрЗаменить(ТелоЗапросаКурл, Символы.ВК, "");
	//end 2024.01.29

	//получим адрес для отправки
	РесурсНаСервере = "/api/proxy/payment/create_pred";

	Кодировка = КодировкаТекста.UTF8;
	УдалитьBOM = Истина;
	UserAgentHeaders = ОпределитьUserAgentHeaders();
	
