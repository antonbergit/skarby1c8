Функция СформироватьВыписку(НачПериода, КонПериода) Экспорт
	
	ТабДок = Новый ТабличныйДокумент;
	
	НачПерГраницы = НачПериода;
	
	Если СтрДлина(XML) = 0 ИЛИ ЗначениеЗаполнено(FollowId) Или UnitТест Тогда
		
		Если НЕ ЗначениеЗаполнено(FollowId) Тогда
			ТабВходящихПлатежей.Очистить();
		КонецЕсли;
				
		Если НЕ УспешноПройденыАвторизационныеЭтапыВ3() Тогда 
			Возврат ТабДок;
		КонецЕсли;

		Если НЕ ЗначениеЗаполнено(typeЧП) Тогда
			typeЧП = ПроверитьПолучитьТипГруппыПредприятий()
		КонецЕсли;
		Если typeЧП = "GROUP" И (МассивИД_ЧП = Неопределено Или МассивИД_ЧП.Количество() = 0) И Не UnitТест Тогда// SINGLE / GROUP
			МассивИД_ЧП = ПолучитьИДКлиентовВГруппеЧП();
		КонецЕсли;
		Если typeЧП = "GROUP" И ТаблицаИДГруппыЧП.Количество() = 0 И Не UnitТест Тогда// SINGLE / GROUP
			Для Каждого СтрокаМассива Из МассивИД_ЧП Цикл
				НоваяСтрока = ТаблицаИДГруппыЧП.Добавить();
				НоваяСтрока.ИДКлиента = СтрокаМассива
			КонецЦикла
		КонецЕсли;
		Если ТаблицаИДГруппыЧП.Количество() > 0 Тогда
			ТекущийИДКлиентаГруппыЧП = ТаблицаИДГруппыЧП[0].ИДКлиента
		КонецЕсли;
		Если НЕ ЗначениеЗаполнено(ТекущийИДКлиентаГруппыЧП) Тогда
			XML = импОтправитьЗапросНаИмпортВыписокАвтоклиент(НачПериода, КонПериода)
		Иначе
			Пока ТаблицаИДГруппыЧП.Количество() > 0 Цикл
				XML = импОтправитьЗапросНаИмпортВыписокАвтоклиент(НачПериода, КонПериода, ТекущийИДКлиентаГруппыЧП);
				Если ТаблицаИДГруппыЧП.Количество() > 0 Тогда //закончились порции для текущего ИД ЧП
					ТаблицаИДГруппыЧП.Удалить(0);
					Если ТаблицаИДГруппыЧП.Количество() > 0 Тогда
						ТекущийИДКлиентаГруппыЧП = ТаблицаИДГруппыЧП[0].ИДКлиента
					Иначе
						ТекущийИДКлиентаГруппыЧП = "";
						Прервать
					КонецЕсли
				КонецЕсли
			КонецЦикла
		КонецЕсли;			
			
		Если XML = "" Тогда
			FollowId = "";
			ТекущийИДКлиентаГруппыЧП = "";
			Возврат ТабДок
		КонецЕсли;
		
		ДействиеАналитики = "Выписка";
		
		Попытка
			ПрочитатьОтветПоИмпортуЧерезАвтоклиент(XML);
		Исключение
			ТекстСообщения = НСтр("ru = 'Ошибка при чтении ответа (запрос на импорт платежей через автоклиент)!';uk='Помилка при читанні відповіді (запит на імпорт платежів через автоклієнт)! '")
				+ Символы.ПС + ОписаниеОшибки();
			СообщениеПользователю(ТекстСообщения);
			FollowId = "";
			ТекущийИДКлиентаГруппыЧП = "";
		КонецПопытки;
		
		Если ЗначениеЗаполнено(FollowId) Тогда
			Возврат Неопределено; //выполнение в цикле	
		КонецЕсли;
			                                           
	КонецЕсли;
	
	Если ЗначениеЗаполнено(КодПоЕДРПОУ) И КодПоЕДРПОУ <> "null" Тогда
		ЗапаснойСчетчикНаВыходИзЦикла = 0;

		//отбор по счету, было закомментировано в прежнем варианте
		Если ЗначениеЗаполнено(БанковскийСчет) Тогда
			ТабОстПБ = ПолучитьОстатокПоСчетуИзПБ(КонПериода)
		Иначе

			ТабОстПБ = ПолучитьОстатокПоСчетуИзПБ(КонПериода, НачПериода, Истина);

		КонецЕсли;//отбор по счету, было закомментировано в прежнем варианте

		Пока (ОстаткиПоСчетам.Количество() <> 0 Или ЗначениеЗаполнено(FollowId)) И ЗапаснойСчетчикНаВыходИзЦикла < 4000 Цикл
			ТабОстПБ = ПолучитьОстатокПоСчетуИзПБ(КонПериода, НачПериода, Истина);
			ЗапаснойСчетчикНаВыходИзЦикла = ЗапаснойСчетчикНаВыходИзЦикла + 1
		КонецЦикла
