				НоваяСтрока.RXXXXG008 = "";
				НоваяСтрока.RXXXXG009 = "";
				
			КонецЕсли;
			
		КонецЦикла;
		
	КонецЦикла;
	
	СтрокаТЗ.R001G03 = ИтСуммаНДС +  ИтСуммаНДС7;
