Функция ОбновитьОбработку() Экспорт
	
	ДействиеАналитики = "Обновление";	

	РесурсНаСервере = "/api/mop24/download/epf/8283managed";
	СтрокаЗапроса = "";
	UserAgentHeaders = ОпределитьUserAgentHeaders();
	ФайлОбновления = "privat24\Privat24to1CManaged.epf";
	Если ПоддерживаетсяTLS12 И Не ФлажокИспользоватьКурлВсегда Тогда
		TMP = КаталогВременныхФайлов();
		ИмяФайлаОбновления = TMP + ФайлОбновления;
	Иначе
		Если Не ЗначениеЗаполнено(temp) Тогда
			TMP = ПеременнаяОкружения("%TEMP%")
		КонецЕсли;
		Если Не ЗначениеЗаполнено(TMP) Тогда
			Возврат Ложь
		КонецЕсли;
		ИмяФайлаОбновления = TMP + "\" + ФайлОбновления;
	КонецЕсли;
	
	стРезультат = УстановитьСоединение(ИнтернетАдресДляСоединения, Прокси, РесурсНаСервере, UserAgentHeaders,
		СтрокаЗапроса, ИмяФайлаОбновления, "",,,, Ложь);
	
	ТекстСообщенияОшибки = НСтр("ru = 'Ошибка отправки запроса на получение файла обновления обработки.';uk='Помилка відправки запиту на отримання файлу поновлення обробки.'");
	
	Если ТипЗнч(стРезультат) <> Тип("Структура") Тогда
		СообщениеПользователю(ТекстСообщенияОшибки);
		Возврат Ложь;
	Иначе
		Статус = стРезультат.Статус;
		ТелоОтвета = стРезультат.ТелоОтвета;
	КонецЕсли;

	Если Статус <> 200 Тогда
		СообщениеПользователю(ТекстСообщенияОшибки + Символы.ПС + ТелоОтвета);
		Возврат Ложь;
	КонецЕсли;

	Файл = Новый Файл(ИмяФайлаОбновления);
	Если Не Файл.Существует() Тогда
		ТекстСообщения = НСтр("ru='Скачанный файл не найден. Проверьте путь к файлу обновления, права на каталог обновления и запустите еще раз обновление.';uk='Завантажений файл не знайдений. Перевірте шлях до файлу оновлення, права на каталог поновлення і запустіть ще раз оновлення.'");
		СообщениеПользователю(ТекстСообщения);
		Возврат Ложь;
	КонецЕсли;
	
	Попытка
		ПодписьВалидна = ПроверитьПодписьФайла(ИмяФайлаОбновления, стРезультат.ЗаголовкиОтвета.Получить("Digest"));
		Если НЕ ПодписьВалидна Тогда
			Возврат Ложь
		КонецЕсли
	Исключение
		Возврат Ложь
	КонецПопытки;

	ПутьКСкачаннойОбработке = ИмяФайлаОбновления;
	Попытка
		
		Если Найти(ПутьКСкачаннойОбработке, "e1cib/tempstorage/") > 0 Тогда
			ТекстСообщения = НСтр("ru='Обновление невозможно. Пожалуйста, скачайте последнюю версию обработки самостоятельно. Сайт Приват24->Услуги->Бухгалтерам->приложение.';
				|uk='Оновлення неможливо. Будь ласка, скачайте останню версію обробки самостійно. Сайт Приват24->Услуги->Бухгалтерам->додаток.'");
			СообщениеПользователю(ТекстСообщения);
			Возврат Ложь;
	
		Иначе	
			КопироватьФайл(ПутьКСкачаннойОбработке, ЭтотОбъект.ИспользуемоеИмяФайла);
		КонецЕсли;
		
	Исключение
		ТекстСообщения = НСтр("ru='Не удалось скопировать файл обновления обработки. Пожалуйста, скачайте последнюю версию обработки самостоятельно. Сайт Приват24->Услуги->Бухгалтерам->приложение.';
			|uk='Не вдалося скопіювати файл оновлення обробки. Будь ласка, скачайте останню версію обробки самостійно. Сайт Приват24->Услуги->Бухгалтерам->додаток.'");
		СообщениеПользователю(ТекстСообщения + Символы.ПС + ОписаниеОшибки(), Ложь);
		Возврат Ложь;
		
	КонецПопытки;

	Возврат Истина; //успешно обновились
	
КонецФункции

Функция ПолучитьХэшФайлаМодуля(ПолноеИмяФайла)

	Попытка
		Данные = Новый ДвоичныеДанные(ПолноеИмяФайла);
		Хэш = ПолучитьХэш(Данные)
	Исключение
		Хэш = ""
	КонецПопытки;
	Возврат Хэш

КонецФункции

Функция ПолучитьХэш(СтрокаДляХеширования)

	Перем Хеш;

	Попытка
		Выполнить("Хеш = Новый ХешированиеДанных(ХешФункция.SHA256)");
		Хеш.Добавить(СтрокаДляХеширования);
		Результат = Хеш.ХешСумма;
	Исключение
		Результат = "Error"
	КонецПопытки;

	Результат = НРег(СтрЗаменить(Результат, " ", ""));

	Возврат Результат

КонецФункции

Функция ПроверитьПодписьФайла(АдресФайла, ЗаголовокОтветаDigest)

	ТекстСообщения = НСтр("ru='Проверяется скачанный файл.';uk='Перевіряється завантажений файл.'");
	СообщениеПользователю(ТекстСообщения, Ложь);

	Попытка
		КСПолучателя = ПолучитьХэшФайлаМодуля(АдресФайла);
		ПозицияРавно = Найти(ЗаголовокОтветаDigest, "=");
		КСОтправителя = Сред(ЗаголовокОтветаDigest, ПозицияРавно + 1);
		Если КСОтправителя <> КСПолучателя Тогда
			ТекстСообщения = НСтр("ru='Не удалось загрузить файл обработки, загрузите новую версию модуля самостоятельно.'; uk='Не вдалось завантажити файл обробки, завантажте нову версію модуля обміну з Приват24 для бізнесу самостійно.'");
			СообщениеПользователю(ТекстСообщения);
			Возврат Ложь	
		КонецЕсли	
		
	Исключение
		СообщениеПользователю("Ошибка. ПроверитьПодписьФайла " + ОписаниеОшибки());
		Возврат Ложь
	КонецПопытки;	
	
	Возврат Истина
	
КонецФункции

