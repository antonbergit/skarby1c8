			ИтогоТабл.Параметры.СуммаДебет = 0;
			ИтогоТабл.Параметры.СуммаКредит = 0;

			//ТабОстПБ = ПолучитьОстатокПоСчетуИзПБ(КонПериода, НачПериода, Истина);//было закомментировано в прежнем варианте

			Если ТабОстПБ.Количество() > 0 Тогда
				СтрокаРСчет = ТабОстПБ.Найти(СтрокаСчета.РСчет, "НомерСчета");
				Если СтрокаРСчет <> Неопределено
						//Или ТабОстПБ.Количество() = 1 И ЗначениеЗаполнено(БанковскийСчет)//было закомментировано в прежнем варианте
						Тогда

					//было закомментировано в прежнем варианте
					//Если ТабОстПБ.Количество() = 1 И ЗначениеЗаполнено(БанковскийСчет) Тогда
					//	СтрокаРСчет = ТабОстПБ[0]//СтрокаСчета.РСчет
					//КонецЕсли;

					НачОстаток = Формат(СтрокаРСчет.СуммаНачОстаток, "ЧДЦ=2; ЧРД=.; ЧГ=; ЧН=");
					КонОстаток = Формат(СтрокаРСчет.СуммаОстаток, "ЧДЦ=2; ЧРД=.; ЧГ=; ЧН=");
