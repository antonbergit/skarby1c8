Функция ПолучениеВыпискаПоКартам(ВыпискаПоГруппеКарт, branch, corporation, corpType, ТекДата, ТекДатаПо, PanNumber) Экспорт

	Если Не UnitТест Тогда
		ТекстВыпискаПо = ?(ВыпискаПоГруппеКарт, НСтр("ru=' по группе карт ';uk=' по групі карт '")
			, НСтр("ru=' по карте ';uk=' по карті '")) + corporation;
		ТекстВидаОбъектаРППусто = НСтр("ru='Выписка пуста';uk='Виписка порожня'") + ТекстВыпискаПо
	КонецЕсли;

	стРезультат = Новый Структура;
	ТекстСообщения = "";

	ДействиеАналитики = "ВыпискаПоКартамАК";
	РесурсНаСервере = "/api/corpcards/v2/corp/statements";
	КлючТипаКарты = corpType;
	Если ВыпискаПоГруппеКарт Тогда
		// 3. Виписка за групою карток
		ТелоЗапроса = "{""corporation"":{""id"":""" + branch + """,""code"":""" + corporation + """,""type"":""" + КлючТипаКарты + """}}"
	Иначе
		// 4. Виписка за карткою
		ТелоЗапроса = "{""corporation"":{""id"":""" + branch + """,""code"":""" + corporation + """,""type"":""" + КлючТипаКарты
			+ """},""clientCard"":""" + PanNumber + """}"
	КонецЕсли;
	РесурсНаСервере = РесурсНаСервере + "?dateStart=" + Формат(ТекДата, "ДФ=дд.ММ.гггг") + "&dateEnd="
			+ Формат(ТекДатаПо, "ДФ=дд.ММ.гггг") + "&" + Получитьrequest_id();
	ТелоЗапросаКурл = ЭкранироватьДвойныеКавычки(ТелоЗапроса);

	Заголовки = Новый ТаблицаЗначений;
	Заголовки.Колонки.Добавить("Заголовок");
	Заголовки.Колонки.Добавить("ЗначениеЗаголовка");

	НоваяСтрока = Заголовки.Добавить();
	НоваяСтрока.Заголовок = "Content-Type";
	НоваяСтрока.ЗначениеЗаголовка = "application/json;charset=cp1251";

	Если ЗначениеЗаполнено(ИдАК) Тогда
		НоваяСтрока = Заголовки.Добавить();
		НоваяСтрока.Заголовок = "id";
		НоваяСтрока.ЗначениеЗаголовка = ИдАК;
	КонецЕсли;

	НоваяСтрока = Заголовки.Добавить();
	НоваяСтрока.Заголовок = "token";
	НоваяСтрока.ЗначениеЗаголовка = ТокенАК;

	АдресВыпискаКорп = АдресАК;
	Кодировка = Неопределено;
	POST = Истина;
	УдалитьBOM = Ложь;
	UserAgentHeaders = ОпределитьUserAgentHeaders();
	Для i = 1 По 2 Цикл

		РезультатСоединения = УстановитьСоединение(ИнтернетАдресДляСоединения, Прокси, РесурсНаСервере, UserAgentHeaders,
			ТелоЗапроса, "", Сессия, Заголовки, Кодировка, , POST, , , УдалитьBOM, ТелоЗапросаКурл, ); 

		Если Не ФлажокЗаменитьОтветСервера И Не UnitТест Тогда
			Если ТипЗнч(РезультатСоединения) <> Тип("Структура") Тогда
				ТекстСообщения = ТекстСообщения + ?(ЗначениеЗаполнено(ТекстСообщения), Символы.ПС, "")
					+ НСтр("ru='Ошибка соединения. Получение выписки корпкартам не выполнено.';uk='Помилка з`єднання. Отримання виписки по корпкартам не виконане.'");
				Продолжить
			КонецЕсли;
			Если ТипЗнч(РезультатСоединения.Статус) = Тип("Строка") Тогда
				Попытка
					СтатусОтвета = Число(Лев(РезультатСоединения.Статус, 3))
				Исключение
					СтатусОтвета = РезультатСоединения.Статус
				КонецПопытки
			Иначе
				СтатусОтвета = РезультатСоединения.Статус
			КонецЕсли;
			Если Найти("401,403,423", СтатусОтвета) > 0 Тогда
				стРезультат.Вставить("ТекстСообщения", ТекстСообщения);
				стРезультат.Вставить("Успешно", Ложь);
				Возврат стРезультат
			ИначеЕсли Найти(СтатусОтвета, "200") > 0 Или Найти(СтатусОтвета, "201") > 0 Тогда
				Прервать
			КонецЕсли
		Иначе
			Прервать
		КонецЕсли
	КонецЦикла;

	Если Не ФлажокЗаменитьОтветСервера И Не UnitТест Тогда
		Если ЗначениеЗаполнено(РезультатСоединения.ТелоОтвета) Тогда
			ТекстОшибки = АнализИВыводОшибокАК(СтатусОтвета, РезультатСоединения.ТелоОтвета);
			Если ЗначениеЗаполнено(ТекстОшибки) Тогда
				СообщениеПользователю(ТекстОшибки, Ложь)
			КонецЕсли
		КонецЕсли;
		Если Найти(СтатусОтвета, "200") = 0 И Найти(СтатусОтвета, "201") = 0 Тогда
			ТекстСообщения = ТекстСообщения + ?(ЗначениеЗаполнено(ТекстСообщения), Символы.ПС, "")
					+ НСтр("ru='Ошибка получения выписки корпкартам.';uk='Помилка отримання виписки по корпкартам.'")
				+ " Статус " + РезультатСоединения.Статус
				+ ?(ЗначениеЗаполнено(РезультатСоединения.ТелоОтвета), ", Ответ " + РезультатСоединения.ТелоОтвета, "");
			стРезультат.Вставить("ТекстСообщения", ТекстСообщения);
			стРезультат.Вставить("Успешно", Ложь);
			Возврат стРезультат
		КонецЕсли;

		СтруктураКорпорацийИКарт = jsonПрочитатьИнициализация(РезультатСоединения.ТелоОтвета)
	Иначе
		Если Не ЗначениеЗаполнено(ПолеВводаТест1) Тогда
			Если Не UnitТест Тогда
				ТекстСообщения = НСтр("ru='Заполните ответ сервера в поле Json транзакций на вкладке Настройки / Общие настройки.';uk='Заповніть відповідь сервера в поле Json транзакцій на вкладці Налаштування / Загальні налаштування.'")
					+ Символы.ПС + НСтр("ru='Для получения выписки по корпкартам из Приват24 снимите флажок Настройки / Общие настройки / ЗаменаОтвета в режиме отладки.';uk='Для отримання виписки по корпкартам з Приват24 зніміть прапорець Налаштування / Загальні налаштування / ЗамінаВідповіді в режимі налагодження.'");
				СообщениеПользователю(ТекстСообщения)
			КонецЕсли;
			стРезультат.Вставить("ТекстСообщения", "");
			стРезультат.Вставить("Успешно", Ложь);
			Возврат стРезультат
		КонецЕсли;
		СтруктураКорпорацийИКарт = jsonПрочитатьИнициализация(ПолеВводаТест1)
	КонецЕсли;

	стРезультатПодготовка = ПодготовкаЗаполнитьКорпКарты(ТекстВидаОбъектаРППусто, СтруктураКорпорацийИКарт);
	Успешно = стРезультатПодготовка.Успешно;
	ТекстСообщения = стРезультатПодготовка.ТекстСообщения;
	Если ЗначениеЗаполнено(ТекстСообщения) Тогда
		СообщениеПользователю(ТекстСообщения, Ложь)
	КонецЕсли;

	Попытка
