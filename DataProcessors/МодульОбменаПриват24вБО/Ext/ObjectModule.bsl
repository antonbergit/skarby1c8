			ЭтоГруппаКарт = Истина
		КонецПопытки;
		statements = мКорпорации.statements;
		Для каждого statement Из statements Цикл
			ПозицияРавно = Найти(statement.account.card, "=");
			НомерКарты = Лев(statement.account.card, ПозицияРавно - 1);
			Если Не ЗначениеЗаполнено(НомерКарты) Тогда
				Продолжить
			КонецЕсли;
			Если ЭтоГруппаКарт Тогда
				ФИОДержателяКарты = "";
				Для каждого эмpanInfos Из panInfos Цикл
					Если statement.account.card = эмpanInfos.pan Тогда
						ФИОДержателяКарты = эмpanInfos.fio;
						Прервать
