	Иначе	
		//НомОб.ТипНоменклатуры = Справочники.ТипыНоменклатуры.Штучный;//существует выбор
		НомОб.ВидНоменклатуры = Перечисления.ВидыНоменклатуры.Товар
		//НомОб.ОсновнаяЕдиницаИзмерения = Справочники.ЕдиницыИзмерения.;//нет предопределенных значений
	КонецЕсли;	

	НоменклатураСсылка = ЗаписатьНоменклатуру(НомОб);
	
	Возврат НоменклатураСсылка;
	
