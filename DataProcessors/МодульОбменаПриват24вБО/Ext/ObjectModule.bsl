Функция UNF_ЗаполнитьСтруктуруФинДок(СсылкаНаДокумент, ИмяДокумента)
	
	ИмяДокПодстановки = СсылкаНаДокумент.Метаданные().Имя;
	Если ФлагСчетИзЗаказа И ТекВидОперации = "Счета" И Лев(ИмяДокПодстановки, 5) = "Заказ" Тогда
		ИмяДокумента = ИмяДокПодстановки
	КонецЕсли;

	Если ИдКонф = "UNF" Тогда
		Если ТекВидОперации = "Заказы" Тогда
			тДокОснование = "	ВЫБОР КОГДА Док.ЗаказПокупателя <> Значение(Документ.ЗаказПокупателя.ПустаяСсылка)
							|				ТОГДА Док.ЗаказПокупателя ИНАЧЕ """" КОНЕЦ КАК ДокОснование,"
		ИначеЕсли ТекВидОперации = "Счета" И Лев(ИмяДокумента, 5) = "Заказ" Тогда
			тДокОснование = "	ВЫБОР КОГДА Док.ДокументОснование <> Значение(Документ.СчетНаОплату.ПустаяСсылка)
							|				ТОГДА Док.ДокументОснование ИНАЧЕ """" КОНЕЦ КАК ДокОснование,"
		ИначеЕсли ТекВидОперации = "Счета" Тогда
			тДокОснование = "	ВЫБОР КОГДА Док.ДокументОснование ССЫЛКА Документ.РасходнаяНакладная
							|				И Док.ДокументОснование <> Значение(Документ.РасходнаяНакладная.ПустаяСсылка)
							|			ИЛИ Док.ДокументОснование ССЫЛКА Документ.ЗаказПокупателя
							|				И Док.ДокументОснование <> Значение(Документ.ЗаказПокупателя.ПустаяСсылка)
							|			ИЛИ Док.ДокументОснование ССЫЛКА Документ.АктВыполненныхРабот
							|				И Док.ДокументОснование <> Значение(Документ.АктВыполненныхРабот.ПустаяСсылка)
							|				ТОГДА Док.ДокументОснование ИНАЧЕ """" КОНЕЦ КАК ДокОснование,"
		//// если Акт - АктВыполненныхРабот//пока работаем с РН
		//ИначеЕсли Лев(ТекВидОперации, 3) = "Акт" Тогда
		//	тДокОснование = "	ВЫБОР КОГДА Док.ДокументОснование <> Значение(Документ.СчетНаОплату.ПустаяСсылка)
		//					|				ТОГДА Док.ДокументОснование ИНАЧЕ
		//					|		ВЫБОР КОГДА Док.ЗаказПокупателя <> Значение(Документ.ЗаказПокупателя.ПустаяСсылка)
		//					|					ТОГДА Док.ЗаказПокупателя ИНАЧЕ """" КОНЕЦ КОНЕЦ КАК ДокОснование,"
		Иначе//РасходнаяНакладная
			тДокОснование = "	ВЫБОР КОГДА Док.ДокументОснование ССЫЛКА Документ.ПриходнаяНакладная
							|				И Док.ДокументОснование <> Значение(Документ.ПриходнаяНакладная.ПустаяСсылка)
							|			ИЛИ Док.ДокументОснование ССЫЛКА Документ.СчетНаОплату
							|				И Док.ДокументОснование <> Значение(Документ.СчетНаОплату.ПустаяСсылка)
							|				ТОГДА Док.ДокументОснование ИНАЧЕ """" КОНЕЦ КАК ДокОснование,"
		КонецЕсли
	Иначе//Если ИдКонф = "BSB" Тогда
		Если ТекВидОперации = "Заказы" Тогда
			тДокОснование = "	ВЫБОР КОГДА Док.ЗаказПокупателя <> Значение(Документ.ЗаказПокупателя.ПустаяСсылка)
							|				ТОГДА Док.ЗаказПокупателя ИНАЧЕ """" КОНЕЦ КАК ДокОснование,"
		ИначеЕсли ТекВидОперации = "Счета" И Лев(ИмяДокумента, 5) = "Заказ" Тогда
			тДокОснование = "	ВЫБОР КОГДА Док.ДокументОснование <> Значение(Документ.СчетНаОплату.ПустаяСсылка)
							|				ТОГДА Док.ДокументОснование ИНАЧЕ """" КОНЕЦ КАК ДокОснование,"
		ИначеЕсли ТекВидОперации = "Счета" Тогда
			тДокОснование = "	ВЫБОР КОГДА Док.ДокументОснование ССЫЛКА Документ.РасходнаяНакладная
							|				И Док.ДокументОснование <> Значение(Документ.РасходнаяНакладная.ПустаяСсылка)
							|			ИЛИ Док.ДокументОснование ССЫЛКА Документ.ЗаказПокупателя
							|				И Док.ДокументОснование <> Значение(Документ.ЗаказПокупателя.ПустаяСсылка)
							|			ИЛИ Док.ДокументОснование ССЫЛКА Документ.АктВыполненныхРабот
							|				И Док.ДокументОснование <> Значение(Документ.АктВыполненныхРабот.ПустаяСсылка)
							|				ТОГДА Док.ДокументОснование ИНАЧЕ """" КОНЕЦ КАК ДокОснование,"
		//// если Акт - АктВыполненныхРабот//пока работаем с РН
		//ИначеЕсли Лев(ТекВидОперации, 3) = "Акт" Тогда
		//	тДокОснование = "	ВЫБОР КОГДА Док.ДокументОснование <> Значение(Документ.СчетНаОплату.ПустаяСсылка)
		//					|				ТОГДА Док.ДокументОснование ИНАЧЕ
		//					|		ВЫБОР КОГДА Док.ЗаказПокупателя <> Значение(Документ.ЗаказПокупателя.ПустаяСсылка)
		//					|					ТОГДА Док.ЗаказПокупателя ИНАЧЕ """" КОНЕЦ КОНЕЦ КАК ДокОснование,"
		Иначе//РасходнаяНакладная и Акт-РасходнаяНакладная
			тДокОснование = "	ВЫБОР КОГДА Док.ДокументОснование ССЫЛКА Документ.ПриходнаяНакладная
							|				И Док.ДокументОснование <> Значение(Документ.ПриходнаяНакладная.ПустаяСсылка)
							|			ИЛИ Док.ДокументОснование ССЫЛКА Документ.СчетНаОплату
							|				И Док.ДокументОснование <> Значение(Документ.СчетНаОплату.ПустаяСсылка)
							// в BASSmallBusinessFlyDoc и BASSmallBusiness1.6.19.4 есть в РН рекв.Док.ДокументОснование в т.ч. типа Документ.ЗаказПоставщику
							// в BASSmallBusiness1.6.15.1 нет этого рекв. При запросе клиента завернуть запрос в ПопыткаИсключение и Цикл
							//|			ИЛИ Док.ДокументОснование ССЫЛКА Документ.ЗаказПоставщику
							//|				И Док.ДокументОснование <> Значение(Документ.ЗаказПоставщику.ПустаяСсылка)
							|				ТОГДА Док.ДокументОснование ИНАЧЕ """" КОНЕЦ КАК ДокОснование,"
		КонецЕсли
	КонецЕсли;

	Запрос = Новый Запрос;
	Запрос.Текст = 
		"ВЫБРАТЬ РАЗРЕШЕННЫЕ
		|	Док.Номер КАК Номер,
		|	Док.Дата КАК Дата,"
		+ тДокОснование + "
		|	Док.Договор.НомерДоговора КАК НомерДоговора,
		|	Док.Договор.ДатаДоговора КАК ДатаДоговора," + 
		?(Метаданные.Справочники.Организации.Реквизиты.Найти("ЮридическоеФизическоеЛицо") = Неопределено, 
		"ВЫБОР КОГДА Док.Организация.ЮрФизЛицо = Значение(Перечисление.ЮрФизЛицо.ЮрЛицо) Тогда ""Юридична"" ИНАЧЕ ""Фізична"" КОНЕЦ КАК Орг_ЮрФизЛицо, ", 
		"ВЫБОР КОГДА Док.Организация.ЮридическоеФизическоеЛицо = Значение(Перечисление.ЮридическоеФизическоеЛицо.ЮридическоеЛицо) Тогда ""Юридична"" ИНАЧЕ ""Фізична"" КОНЕЦ КАК Орг_ЮрФизЛицо, ") + 
		"	ВЫБОР КОГДА ВЫРАЗИТЬ(Док.Организация.НаименованиеПолное КАК СТРОКА(250)) = """" ТОГДА Док.Организация.Наименование ИНАЧЕ
