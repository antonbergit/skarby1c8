Функция ПроверитьПолучитьТипГруппыПредприятий() Экспорт

	ПодразделениеГруппы.Очистить();
	ДействиеАналитики = "ПолучениеПризнакаКлиентаГруппаЧП";
	РесурсНаСервере = "/api/account/get-account-info";
	
	Заголовки = Новый ТаблицаЗначений;
	Заголовки.Колонки.Добавить("Заголовок");
	Заголовки.Колонки.Добавить("ЗначениеЗаголовка");
	
	Если ЗначениеЗаполнено(ИдАК) Тогда
		НоваяСтрока = Заголовки.Добавить();
		НоваяСтрока.Заголовок = "id";
		НоваяСтрока.ЗначениеЗаголовка = ИдАК;
	КонецЕсли;
	
	НоваяСтрока = Заголовки.Добавить();
	НоваяСтрока.Заголовок = "token";
	НоваяСтрока.ЗначениеЗаголовка = ТокенАК;
	
	НоваяСтрока = Заголовки.Добавить();
	НоваяСтрока.Заголовок = "Content-Type";
	НоваяСтрока.ЗначениеЗаголовка = "application/json;charset=utf-8";
	
	ТелоЗапроса = "";
	ТелоЗапросаКурл = "";
	ИмяФайлаРезультата = "";
	POST = Ложь;
	УдалитьBOM = Ложь;
	UserAgentHeaders = ОпределитьUserAgentHeaders();
	
	РезультатСоединения = УстановитьСоединение(АдресАК, Прокси, РесурсНаСервере, UserAgentHeaders, ТелоЗапроса, ИмяФайлаРезультата, "", Заголовки,//Кодировка,
		,, POST, , , УдалитьBOM, ТелоЗапросаКурл);
	
	Если РезультатСоединения = Неопределено	Или РезультатСоединения = Ложь Тогда
		ТекстСообщения = НСтр("ru='Ошибка соединения!';uk='Помилка з`єднання!'");
		СообщениеПользователю(ТекстСообщения);
		Возврат ""
	КонецЕсли;
	Если Не UnitТест И Найти(РезультатСоединения.Статус, "200") = 0 И Найти(РезультатСоединения.Статус, "201") = 0 Тогда
		ТекстСообщения = НСтр("ru='Ошибка ';uk='Помилка '") + РезультатСоединения.Статус;
		СообщениеПользователю(ТекстСообщения)
	ИначеЕсли Не UnitТест Тогда
		ОбновитьСтатистикуРаботы(КатегорияПоСобытию(), ДействиеАналитики)
	КонецЕсли;
	Если РезультатСоединения = Ложь Тогда
		ТекстСообщения = НСтр("ru='Ошибка соединения. Информация по организации не получена. ';uk='Помилка з'єднання. Інформація по організації не отримана. '");
		СообщениеПользователю(ТекстСообщения);
	КонецЕсли;
	// обработка ответа
	Если Не UnitТест И Не ЗначениеЗаполнено(РезультатСоединения.ТелоОтвета) Тогда
		ТекстСообщения = НСтр("ru='Ответ сервера пуст. Информация по организации не получена. ';uk='Відповідь сервера порожня. Інформація по організації не отримана. '");
		СообщениеПользователю(ТекстСообщения);
	КонецЕсли;
	Если Не UnitТест Тогда
		Попытка
			СтруктураОтвета = jsonПрочитатьИнициализация(РезультатСоединения.ТелоОтвета);
			typeЧП = СтруктураОтвета.type
		Исключение
		КонецПопытки
	Иначе
		typeЧП = "SINGLE"
	КонецЕсли;

	Возврат typeЧП

