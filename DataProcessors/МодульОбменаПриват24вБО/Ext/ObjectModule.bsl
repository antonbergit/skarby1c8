Функция AAA_СоздатьНомеклатуру(Наименование, Артикул, ЕдИзм, СтруктураПараметров)
	
	НомОб = Справочники.Номенклатура.СоздатьЭлемент();
	НомОб.Наименование = Наименование;
	НомОб.НаименованиеПолное = Наименование;
	НомОб.Артикул = Артикул;
	
	Если ЗначениеЗаполнено(СтруктураПараметров.CтавкаПДВ) Тогда
		Попытка
			НомОб.СтавкаНДС = ПолучитьСтавкуНДСПоСтроковомуПредставлению(СтруктураПараметров.CтавкаПДВ)
		Исключение
		КонецПопытки
	КонецЕсли;

	НомОб.БазоваяЕдиницаИзмерения = НайтиСоздатьЕдиницуИзмерения(ЕдИзм);
	НомОб.ВалютаУчета = Константы.ВалютаРегламентированногоУчетаОрганизаций.Получить();

	Если СтруктураПараметров.Тип = "Послуга" ИЛИ (СтрЗаменить(НРег(ЕдИзм),".","") = "посл" ИЛИ СтрЗаменить(НРег(ЕдИзм),".","") = "усл") Тогда
		НомОб.ТипНоменклатуры = Справочники.ТипыНоменклатуры.Услуга;
