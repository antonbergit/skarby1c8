				СтрокаОбъектДокументаВыпискаПоРасчетномуСчету = ОбъектДокументаВыпискаПоРасчетномуСчетуТаблЧасть.Добавить();
				СтрокаОбъектДокументаВыпискаПоРасчетномуСчету.ПлатежноеПоручение = ТаблицаДокументовКИмпорту[0].СсылкаНаДокумент;
				
				ВидОперации = ТаблицаДокументовКИмпорту[0].ВидОперации;
				Если Не ЗначениеЗаполнено(ВидОперации) Тогда
					ВидОперации = Справочники.ВидыОпераций.НайтиПоНаименованию("Платіжне доручення")
				КонецЕсли;
				УстановитьСвойство(СтрокаОбъектДокументаВыпискаПоРасчетномуСчету, "ВидОперации", ВидОперации);
				//УстановитьСвойство(СтрокаОбъектДокументаВыпискаПоРасчетномуСчету, "КорреспондирующийСчет", ТаблицаДокументовКИмпорту[0].СчетКонтрагента);
				УстановитьСвойство(СтрокаОбъектДокументаВыпискаПоРасчетномуСчету, "СтавкаНДС", ТаблицаДокументовКИмпорту[0].СтавкаНДС);
				УстановитьСвойство(СтрокаОбъектДокументаВыпискаПоРасчетномуСчету, "СуммаНДС", ТаблицаДокументовКИмпорту[0].СуммаНДС);
				УстановитьСвойство(СтрокаОбъектДокументаВыпискаПоРасчетномуСчету, "СуммаСНДС", ТаблицаДокументовКИмпорту[0].СуммаДокумента);
				УстановитьСвойство(СтрокаОбъектДокументаВыпискаПоРасчетномуСчету, "СуммаБезНДС", ТаблицаДокументовКИмпорту[0].СуммаДокумента - ТаблицаДокументовКИмпорту[0].СуммаНДС);
				УстановитьСвойство(СтрокаОбъектДокументаВыпискаПоРасчетномуСчету, "Содержание", ТаблицаДокументовКИмпорту[0].НазначениеПлатежа)//НазначениеПлатежа

			КонецЕсли;
			
			ТаблицаДокументовКИмпорту.Удалить(0);
			
			Если ТаблицаДокументовКИмпорту.Количество() = 0 Тогда
				Прервать;
			КонецЕсли;
			
		КонецЦикла;
				
		Если ОбъектДокументаВыпискаПоРасчетномуСчету.Модифицированность()
				И ОбъектДокументаВыпискаПоРасчетномуСчету.Банк.Количество() > 0 Тогда	
			
			Попытка
				
				ОбъектДокументаВыпискаПоРасчетномуСчету.Записать(РежимЗаписиДокумента.Запись);
				ДозаполнитьРеквизитыДокумента(ОбъектДокументаВыпискаПоРасчетномуСчету);					
				Выполнить("Текст = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(НСтр(""ru='%1 документ ""Выписка по банковскому счету"" № %2 от %3.';uk='%1 документ ""Виписка за банківським рахунком"" № %2 від %3.'""), ?(СозданНовыйДокумент, ""Создан"", ""Перезаписан""), ОбъектДокументаВыпискаПоРасчетномуСчету.Номер, ОбъектДокументаВыпискаПоРасчетномуСчету.Дата)");
				СообщениеПользователю(Текст, Ложь);

			Исключение
				СтрокаПодстановки = НСтр("ru='Документ ""Выписка по банковскому счету"" № %1 от %2 не %3! Произошли ошибки при записи!';uk='Документ ""Виписка по банківському рахунку"" № %1 від % 2 не %3! Сталися помилки при запису!'");
				Выполнить("Текст = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(СтрокаПодстановки, ОбъектДокументаВыпискаПоРасчетномуСчету.Номер, ОбъектДокументаВыпискаПоРасчетномуСчету.Дата, ?(СозданНовыйДокумент, ""создан"", ""перезаписан"")) + Символы.ПС + ОписаниеОшибки()");
				СообщениеПользователю(Текст, Ложь);
			КонецПопытки;
			
			Если ОбъектДокументаВыпискаПоРасчетномуСчету.ПометкаУдаления Тогда
				ОбъектДокументаВыпискаПоРасчетномуСчету.УстановитьПометкуУдаления(Ложь);
			КонецЕсли;
			
			Если //ОбъектДокументаВыпискаПоРасчетномуСчету.ПроверитьЗаполнение() И
					ОбъектДокументаВыпискаПоРасчетномуСчету.Дата <= КонецДня(ТекущаяДата()) Тогда
				
				Попытка
					
					ОбъектДокументаВыпискаПоРасчетномуСчету.Записать(РежимЗаписиДокумента.Проведение);
					ДозаполнитьРеквизитыДокумента(ОбъектДокументаВыпискаПоРасчетномуСчету);	
					
					Выполнить("Текст = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(НСтр(""ru='%1 документ ""Выписка по банковскому счету"" № %2 от %3.';uk='%1 документ ""Виписка за банківським рахунком"" № %2 від %3.'""), ?(СозданНовыйДокумент, ""Проведен"", ""Перепроведен""), ОбъектДокументаВыпискаПоРасчетномуСчету.Номер, ОбъектДокументаВыпискаПоРасчетномуСчету.Дата)");
					
					СообщениеПользователю(Текст, Ложь);
										
				Исключение
					СтрокаПодстановки = НСтр("ru='Документ ""Выписка по банковскому счету"" № %1 от %2 не %3! Произошли ошибки при проведении!';uk='Документ ""Виписка з банківського рахунку"" № %1 від % 2 не %3! Сталися помилки при проведенні!'");
					Выполнить("Текст = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(СтрокаПодстановки, ОбъектДокументаВыпискаПоРасчетномуСчету.Номер, ОбъектДокументаВыпискаПоРасчетномуСчету.Дата, ?(СозданНовыйДокумент, ""проведен"", ""перепроведен"")) + Символы.ПС + ОписаниеОшибки()");
					СообщениеПользователю(Текст, Ложь);
				КонецПопытки
				
			Иначе	
				
				Выполнить("Текст = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(НСтр(""ru='Документ ""Выписка по банковскому счету"" № %1 от %2 не %3! Произошли ошибки при проведении!';uk='Документ ""Виписка з банківського рахунку"" № %1 від % 2 не %3! Сталися помилки при проведенні!'""), ОбъектДокументаВыпискаПоРасчетномуСчету.Номер, ОбъектДокументаВыпискаПоРасчетномуСчету.Дата, ?(СозданНовыйДокумент, ""проведен"", ""перепроведен""))");
				СообщениеПользователю(Текст, Ложь);

			КонецЕсли;
			
		ИначеЕсли НЕ ОбъектДокументаВыпискаПоРасчетномуСчету.Ссылка.Пустая()
				И ОбъектДокументаВыпискаПоРасчетномуСчету.Банк.Количество() = 0
		 		//И ОбъектДокументаВыпискаПоРасчетномуСчету..Количество() = 0
				Тогда
			
			ОбъектДокументаВыпискаПоРасчетномуСчету.УстановитьПометкуУдаления(Истина);	
			
		КонецЕсли;
		
	КонецЦикла;
	
КонецПроцедуры

Процедура ЭкспортПлатежей(КонПериода, Автообмен = Ложь) Экспорт
	
	ДействиеАналитики = "ВыгрузкаПлатежей";	
	
	ТЗЭкспорт = отпСформироватьТЗИсходящихПлатежей(); //создаем таблицу значений для экспорта
	
	ЕстьОтмеченные = Ложь;
	Для Каждого СтрокаТЧ Из ЭкспортПлатежныеДокументы Цикл 
		
		Если НЕ ЗначениеЗаполнено(СтрокаТЧ.СсылкаНаДокумент) Тогда
			Продолжить;	
		КонецЕсли;
		
		Если СтрокаТЧ.Флажок Или Автообмен Тогда
			
			Если Не ЗначениеЗаполнено(СтрокаТЧ.НазначениеПлатежа) И Не UnitТест Тогда
				ТекстСообщения = НСтр("ru = 'Внимание! В строке №%НомерСтроки% не заполнено назначение платежа. Документ не выгружен.'; uk = 'Увага! У рядку №%НомерСтроки% не заповнено призначення платежу. Документ не вивантажено.' ");
				ТекстСообщения = СтрЗаменить(ТекстСообщения, "%НомерСтроки%", СтрокаТЧ.НомерСтроки);
				СообщениеПользователю(ТекстСообщения, Ложь);
				СтрокаТЧ.КартинкаОшибки = 0;
				СтрокаТЧ.ТекстОшибки = ТекстСообщения;
				Продолжить;
			КонецЕсли;

			ДлинаНазначениеПлатежа = СтрДлина(СтрокаТЧ.НазначениеПлатежа);
			Если (ДлинаНазначениеПлатежа < 5 Или ДлинаНазначениеПлатежа > 420) И Не UnitТест Тогда
				ТекстСообщения = НСтр("ru = 'Внимание! В строке №%НомерСтроки% длина назначения платежа должна быть в диапазоне от 5 до 420 символов: %НазначениеПлатежа%' ; uk = 'Увага! У рядку №%НомерСтроки% довжина призначення платежу повинна бути в діапазоні від 5 до 420 символів: %НазначениеПлатежа%' ");
				ТекстСообщения = ТекстСообщения + НСтр("ru = '. Документ не выгружен.' ; uk = '. Документ не вивантажено.' ");
				ТекстСообщения = СтрЗаменить(ТекстСообщения, "%НомерСтроки%", СтрокаТЧ.НомерСтроки);
				ТекстСообщения = СтрЗаменить(ТекстСообщения, "%НазначениеПлатежа%", СтрокаТЧ.НазначениеПлатежа);
				СообщениеПользователю(ТекстСообщения, Ложь);
				СтрокаТЧ.КартинкаОшибки = 0;
				СтрокаТЧ.ТекстОшибки = ТекстСообщения;
				Продолжить;
			КонецЕсли;

			Если Не ЗначениеЗаполнено(СтрокаТЧ.СчетКонтрагента) И Не UnitТест Тогда
				ТекстСообщения = НСтр("ru = 'Внимание! В строке №%НомерСтроки% не заполнен расчетный счет. Документ не выгружен.' ; uk = 'Увага! У рядку №%НомерСтроки% не заповнений розрахунковий рахунок. Документ не вивантажено.' ");
				ТекстСообщения = СтрЗаменить(ТекстСообщения, "%НомерСтроки%", СтрокаТЧ.НомерСтроки);
				СообщениеПользователю(ТекстСообщения, Ложь);
				СтрокаТЧ.КартинкаОшибки = 0;
				СтрокаТЧ.ТекстОшибки = ТекстСообщения;
				Продолжить;
			КонецЕсли;
			
			Если ((НЕ СчетВФорматеIBAN(СтрокаТЧ.СчетКонтрагента.НомерСчета) И НЕ ЭтоКарта(СтрокаТЧ.СчетКонтрагента.НомерСчета))
					ИЛИ (НЕ СчетВФорматеIBAN(СтрокаТЧ.БанковскийСчет.НомерСчета) И НЕ ЭтоКарта(СтрокаТЧ.БанковскийСчет.НомерСчета))) И Не UnitТест Тогда
				
				ТекстСообщения = НСтр("ru = 'Внимание! В строке №%НомерСтроки% обнаружен расчетный счет не в формате IBAN. Документ не выгружен.' ; uk = 'Увага! У рядку №%НомерСтроки% виявлений розрахунковий рахунок не в форматі IBAN. Документ не вивантажено.' ");
				ТекстСообщения = СтрЗаменить(ТекстСообщения, "%НомерСтроки%", СтрокаТЧ.НомерСтроки);
				СообщениеПользователю(ТекстСообщения, Ложь);
				СтрокаТЧ.КартинкаОшибки = 0;
				СтрокаТЧ.ТекстОшибки = ТекстСообщения;
				Продолжить;
			КонецЕсли;
			
			ЕстьОтмеченные = Истина;
			
			НоваяСтрока = ТЗЭкспорт.Добавить();
			отпДобавитьДокВТЗИсходящихПлатежей(СтрокаТЧ, НоваяСтрока); //заполнение таблицы экспорта
			НоваяСтрока.СсылкаНаДокумент = СтрокаТЧ.СсылкаНаДокумент
			
		КонецЕсли;	
	КонецЦикла;

	Если НЕ ЕстьОтмеченные И Не Автообмен И Не UnitТест Тогда
		ТекстСообщения = НСтр("ru = 'Нет отмеченных платежных документов для экспорта!'; uk = 'Немає відмічених платіжних документів для експорту!'");
		СообщениеПользователю(ТекстСообщения, Ложь);
		Возврат;
	КонецЕсли;

	Если НЕ УспешноПройденыАвторизационныеЭтапыВ3() Тогда 
		Возврат;
	КонецЕсли;

	Если Не ЗначениеЗаполнено(temp) Тогда
		temp = ПеременнаяОкружения("%TEMP%");
	КонецЕсли;
	Если Не ЗначениеЗаполнено(temp) Тогда
		Возврат
	КонецЕсли;
	СуффиксИмени = СтрокаСоединенияИнформационнойБазы();
	СуффиксИмени = СтрЗаменить(СтрЗаменить(СтрЗаменить(СуффиксИмени, Символ(182), ""), Символ(9), ""), Символы.Таб, "");
	СуффиксИмени = СтрЗаменить(СтрЗаменить(СуффиксИмени, Символы.ПС, ""), Символы.ВК, "");
	СуффиксИмени = СтрЗаменить(СуффиксИмени, Символ(32), "");//	убрать пробелы
	СуффиксИмени = СтрЗаменить(СтрЗаменить(СтрЗаменить(СуффиксИмени, Символы.ПФ, ""), Символы.ВТаб, ""), Символы.НПП, "");
	СуффиксИмени = СтрЗаменить(СтрЗаменить(СуффиксИмени, "'", ""), "`", "");//убрать кавычки
	СуффиксИмени = СтрЗаменить(СуффиксИмени, "\", "");
	СуффиксИмени = СтрЗаменить(СуффиксИмени, "=", "");
	СуффиксИмени = СтрЗаменить(СуффиксИмени, ";", "");
	СуффиксИмени = СтрЗаменить(СуффиксИмени, ",", "");
	СуффиксИмени = СтрЗаменить(СуффиксИмени, ".", "");
	СуффиксИмени = СтрЗаменить(СтрЗаменить(СуффиксИмени, "/", ""), """", "");
	СуффиксИмени = СтрЗаменить(СуффиксИмени, ":", "");

	ПолноеИмяФайлаIDИсх = temp + "\IDИсх" + СуффиксИмени + ".txt";
	ФайлIDИсх = Новый Файл(ПолноеИмяФайлаIDИсх);

	ЕстьОшибки = Ложь;
	//теперь отправка каждого документа отдельно
	КоличествоПлатежей = 0;
	ЕстьНеотправленныеДокументы = Ложь;
	Для Каждого СтрокаТЗ Из ТЗЭкспорт Цикл
		Попытка
			IDДокНайден = НайтиIDДокумента(СтрокаТЗ, ФайлIDИсх, Автообмен);
			Если IDДокНайден И Не ФлагЭкспортироватьПлатежиПовторно Тогда
				ЕстьНеотправленныеДокументы = Истина
			КонецЕсли;
			Если IDДокНайден И Не UnitТест Тогда
				//документ уже был отправлен
				Продолжить
			КонецЕсли;

			ВсеОК = отпОтправитьОдинПлатеж(СтрокаТЗ);

			Если Не UnitТест Тогда
				Если НЕ ВсеОК Тогда
					ЕстьОшибки = Истина;
				ИначеЕсли Не IDДокНайден Тогда
					ЗаписатьIDДокумента(СтрокаТЗ, ФайлIDИсх);
					КоличествоПлатежей = КоличествоПлатежей + 1;
				КонецЕсли
			КонецЕсли;
		Исключение
			ТекстСообщения = НСтр("ru = 'Ошибка при отправке платежа в строке №'; uk = 'Помилка при відправці платежу в рядку №' ")
				+ СтрокаТЗ.НомерСтрокиЭкспорта + Символы.ПС + ОписаниеОшибки();
			СообщениеПользователю(ТекстСообщения);//, Ложь
			ЭкспортПлатежныеДокументы[СтрокаТЗ.НомерСтрокиЭкспорта-1].КартинкаОшибки = 0;
			ЭкспортПлатежныеДокументы[СтрокаТЗ.НомерСтрокиЭкспорта-1].ТекстОшибки = ТекстСообщения;
		КонецПопытки;	
	КонецЦикла;
	
	Если ЕстьОшибки Тогда
		ТекстСообщения = НСтр("ru = 'Ошибка при отправке платежей! Все проблемные строки отмечены в списке.'; uk = 'Помилка при відправці платежів! Всі проблемні рядки відзначені в списку.' ");
		СообщениеПользователю(ТекстСообщения, Ложь);
	Иначе
		Если Не UnitТест Тогда
			ОбновитьСтатистикуРаботы(КатегорияПоСобытию(), ДействиеАналитики ,,,"КоличествоПлатежей", Формат(КоличествоПлатежей, "ЧГ=0"));
			Если ЕстьНеотправленныеДокументы Тогда
				ТекстСообщения = НСтр("ru = 'Не все отмеченные документы выгружены.'; uk = 'Не всі відзначені документи вивантажені.' ")
			Иначе
				ТекстСообщения = НСтр("ru = 'Все отмеченные документы успешно выгружены.'; uk = 'Всі відзначені документи успішно вивантажені.' ")
			КонецЕсли;
			СообщениеПользователю(ТекстСообщения, Ложь);
		КонецЕсли
	КонецЕсли;
	
КонецПроцедуры // ЭкспортПлатежей()

Функция НайтиIDДокумента(Документ, ФайлIDИсх, Автообмен)

	ID = ЗначениеВСтрокуВнутр(Документ.СсылкаНаДокумент);
	НайденID = Ложь;
	ТекстовыйДокумент = Новый ТекстовыйДокумент;
	Если ФайлIDИсх.Существует() Тогда
		ТекстовыйДокумент.Прочитать(ФайлIDИсх.ПолноеИмя);
		Для i = 1 По ТекстовыйДокумент.КоличествоСтрок() Цикл
			СтрокаТекДок = ТекстовыйДокумент.ПолучитьСтроку(i);
			ПозицияНачалаID = Найти(СтрокаТекДок, "{");
			ПозицияКонцаID = Найти(СтрокаТекДок, "}");
			СтрокаНайденногоID = Сред(СтрокаТекДок, ПозицияНачалаID, ПозицияКонцаID - ПозицияНачалаID + 1);
			Если ID = СтрокаНайденногоID Тогда
				НайденID = Истина;
				Прервать
			КонецЕсли
		КонецЦикла;
		Если НайденID Тогда
			Если Автообмен Тогда
				Возврат НайденID
			КонецЕсли;
			Если ФлагЭкспортироватьПлатежиПовторно И Не UnitТест Тогда
				ТекстВопроса = НСтр("ru='Платеж № ';uk='Платіж № '") + СокрЛП(Документ.НомерПоручения)
					+ НСтр("ru=' от ';uk=' від '") + СокрЛП(Документ.ДатаПлатежа)
					+ НСтр("ru=' отправляется повторно.';uk=' відправляється повторно.'");
				СообщениеПользователю(ТекстВопроса, Ложь);
				Возврат Ложь//эмуляция ненайденного документа
			КонецЕсли;
			//	ID найден - не загружаем
			Если Не UnitТест Тогда
				ТекстВопроса = НСтр("ru='Платеж № ';uk='Платіж № '") + СокрЛП(Документ.НомерПоручения)
					+ НСтр("ru=' от ';uk=' від '") + СокрЛП(Документ.ДатаПлатежа)
					+ НСтр("ru=' был отправлен ранее.';uk=' був відправлений раніше.'")
					+ НСтр("ru=' Платеж пропущен.';uk=' Платіж пропущений.'");
					//+ НСтр("ru=' Вы действительно желаете отправить платеж повторно?';uk=' Ви дійсно бажаєте відправити платіж повторно?'");
				СообщениеПользователю(ТекстВопроса, Ложь)
			КонецЕсли;
			// интерактивный контроль повторной отправки платежей не используется
			////Если Вопрос(ТекстВопроса, РежимДиалогаВопрос.ДаНет) = КодВозвратаДиалога.Нет Тогда
			////	Возврат Ложь
			////КонецЕсли;
			//Возврат Истина
		Иначе
			//	ID не найден - загружаем
		КонецЕсли
	Иначе
		//файл ID не существует - загружаем
	КонецЕсли;

	Возврат НайденID

КонецФункции

