Функция UABY_ЗаполнитьСтруктуруФинДок(СсылкаНаДокумент, ИмяДокумента)

	тДокОснование =  """"" КАК ДокОснование,";

	Запрос = Новый Запрос;
	Запрос.Текст = 
		"ВЫБРАТЬ РАЗРЕШЕННЫЕ
		|	Док.Номер КАК Номер,
		|	Док.Дата КАК Дата,"
		+ тДокОснование + "
		|	Док.Договор.НомерДоговора КАК НомерДоговора,
		|	Док.Договор.ДатаДоговора КАК ДатаДоговора,
		|	""Юридична"" КАК Орг_ЮрФизЛицо,
		|	ВЫБОР КОГДА Док.Организация.НаименованиеПолное = """" ТОГДА Док.Организация.Наименование ИНАЧЕ
		|		Док.Организация.НаименованиеПолное КОНЕЦ КАК Орг_НаименованиеПолное,
		|	КодыОрганизаций.КодЕДРПОУ КАК Орг_КодПоЕДРПОУ,
		|	КодыОрганизаций.ИНН КАК Орг_ИНН,
		|	КодыОрганизаций.НомерСвидетельства КАК Орг_НомерСвидетельства,"
		+ ?(ТекВидОперации = "Счета" ИЛИ ТекВидОперации = "Расходные", "Док.РасчетныйСчет.Банк.Код КАК МФО, Док.РасчетныйСчет.НомерСчета КАК НомерСчета, Док.РасчетныйСчет.Банк.Наименование КАК НазваниеБанка,", "
		|	РасчетныеСчета.Банк.Код КАК МФО, РасчетныеСчета.НомерСчета КАК НомерСчета, РасчетныеСчета.Банк.Наименование КАК НазваниеБанка,") + "
		|	ВЫБОР КОГДА Док.Контрагент.ЮридическоеФизическоеЛицо = Значение(Перечисление.ЮридическоеФизическоеЛицо.ЮридическоеЛицо) Тогда ""Юридична"" ИНАЧЕ ""Фізична"" КОНЕЦ КАК Контр_ЮрФизЛицо,
		|	ВЫБОР КОГДА Док.Контрагент.НаименованиеПолное = """" ТОГДА Док.Контрагент.Наименование ИНАЧЕ
		|		Док.Контрагент.НаименованиеПолное КОНЕЦ КАК Контр_НаименованиеПолное,
		|	КодыКонтрагентов.КодЕДРПОУ КАК Контр_КодПоЕДРПОУ,
		|	КодыКонтрагентов.ИНН КАК Контр_ИНН,
		|	КодыКонтрагентов.НомерСвидетельства КАК Контр_НомерСвидетельства,
		|   РасчетныеСчетаКонтрагентов.НомерСчета КАК Контр_НомерСчета,
		|   РасчетныеСчетаКонтрагентов.Банк.Код КАК Контр_МФО,
		|   РасчетныеСчетаКонтрагентов.Банк.Наименование КАК Контр_НазваниеБанка,
		|	Док.Комментарий КАК Комментарий,
		|	ОтветственныеЛицаОрганизаций.Сотрудник КАК ОтветственныйФИО,
		|	ОтветственныеЛицаОрганизаций.Должность КАК ОтветственныйДолжность,
		|	"""" КАК Контр_ОтветственныйФИО,
		|	"""" КАК Контр_ОтветственныйДолжность,
		|	" + ?(ТекВидОперации = "Счета", """""", "ЕСТЬNULL(Док.ВалютаДокумента.Код, Док.Договор.ВалютаДокумента.Код)") + " КАК КодВалюты,
		|	" + ?(ТекВидОперации = "Счета", """""", "Док.КурсДокумента") + " КАК КурсВалюты,
		|	" + ?(ТекВидОперации = "Счета", """""", "Док.КратностьДокумента") + " КАК КратностьВалюты,
		|	0 КАК СуммаБезНДС,
		|	0 КАК СуммаНДС,
		|	0 КАК СуммаСНДС," 
		+ ?(ТекВидОперации = "Расходные", "Док.МестоСоставленияДокумента", " """" ") + " КАК МестоСоставления,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Орг_Телефон,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Орг_ЮрАдреса_Індекс,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Орг_ЮрАдреса_Область,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Орг_ЮрАдреса_Місто,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Орг_ЮрАдреса_Вулиця,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Орг_ФактАдреса_Індекс,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Орг_ФактАдреса_Область, 
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Орг_ФактАдреса_Місто,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Орг_ФактАдреса_Вулиця,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Орг_ПоштАдреса_Індекс, 
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Орг_ПоштАдреса_Область,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Орг_ПоштАдреса_Місто,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Орг_ПоштАдреса_Вулиця,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Контр_Телефон,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Контр_ЮрАдреса_Індекс,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Контр_ЮрАдреса_Область,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Контр_ЮрАдреса_Місто, 
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Контр_ЮрАдреса_Вулиця,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Контр_ФактАдреса_Індекс,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Контр_ФактАдреса_Область,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Контр_ФактАдреса_Місто,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Контр_ФактАдреса_Вулиця,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Контр_ПоштАдреса_Індекс,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Контр_ПоштАдреса_Область,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Контр_ПоштАдреса_Місто,
		|	ВЫРАЗИТЬ ("""" КАК СТРОКА (200)) КАК Контр_ПоштАдреса_Вулиця
		|ИЗ
		|	Документ." + ИмяДокумента + " КАК Док
		|	ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КодыОрганизаций.СрезПоследних(&Дата) КАК КодыОрганизаций
		|		ПО КодыОрганизаций.Организация = Док.Организация
		|	ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ОтветственныеЛицаОрганизаций.СрезПоследних(&Дата) КАК ОтветственныеЛицаОрганизаций
		|		ПО ОтветственныеЛицаОрганизаций.Организация = Док.Организация
		|			И ОтветственныеЛицаОрганизаций.ОтветственноеЛицо = Значение(Перечисление.ОтветственныеЛицаОрганизаций.Руководитель)
		|	ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КодыКонтрагентов.СрезПоследних(&Дата) КАК КодыКонтрагентов
		|		ПО КодыКонтрагентов.Контрагент = Док.Контрагент
		|	ЛЕВОЕ СОЕДИНЕНИЕ Справочник.РасчетныеСчета КАК РасчетныеСчета
		|		ПО РасчетныеСчета.Владелец = Док.Организация
		|	ЛЕВОЕ СОЕДИНЕНИЕ Справочник.РасчетныеСчетаКонтрагентов КАК РасчетныеСчетаКонтрагентов
		|		ПО РасчетныеСчетаКонтрагентов.Владелец = Док.Контрагент
		|ГДЕ
		|	Док.Ссылка = &Ссылка";
	
	Запрос.УстановитьПараметр("Ссылка", СсылкаНаДокумент);
	Запрос.УстановитьПараметр("Дата", СсылкаНаДокумент.Дата);

	ШапкаДок = Запрос.Выполнить().Выгрузить();
		
