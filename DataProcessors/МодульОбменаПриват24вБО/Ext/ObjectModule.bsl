Функция КонвертацияАдресаДляБУ(СтрАдресаВх)

	СтруктураАдреса = Новый Структура;

	Если СтрАдресаВх.Количество() > 0 Тогда

		СтрАдресаВх = СтрАдресаВх[0];
		СтрАдресаВх = jsonПрочитатьИнициализация(СтрАдресаВх.Значение);

		Попытка
			СтруктураАдреса.Вставить("Корпус", СтрАдресаВх.buildings[0].number)
		Исключение
			СтруктураАдреса.Вставить("Корпус", "")
		КонецПопытки;

		СтруктураАдреса.Вставить("Город", СтрАдресаВх.city);
		СтруктураАдреса.Вставить("Индекс", СтрАдресаВх.ZIPcode);
		СтруктураАдреса.Вставить("Страна", СтрАдресаВх.country);
		СтруктураАдреса.Вставить("Улица", СтрАдресаВх.street);
		СтруктураАдреса.Вставить("Регион", СтрАдресаВх.area);
		СтруктураАдреса.Вставить("Дом", СтрАдресаВх.houseNumber);
		СтруктураАдреса.Вставить("Район", СтрАдресаВх.district);
		СтруктураАдреса.Вставить("НаселенныйПункт", СтрАдресаВх.settlement);//locality

		Попытка
			СтруктураАдреса.Вставить("Квартира", СтрАдресаВх.apartments[0].number)
		Исключение
			СтруктураАдреса.Вставить("Квартира", "")
		КонецПопытки

	КонецЕсли;

	Возврат СтруктураАдреса

