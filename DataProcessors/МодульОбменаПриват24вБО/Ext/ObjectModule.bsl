Функция AAA_ЗаполнитьСтруктуруФинДок(СсылкаНаДокумент, ИмяДокумента)

	ИмяДокПодстановки = СсылкаНаДокумент.Метаданные().Имя;
	Если ФлагСчетИзЗаказа И ТекВидОперации = "Счета" И Лев(ИмяДокПодстановки, 5) = "Заказ" Тогда
		ИмяДокумента = ИмяДокПодстановки
	КонецЕсли;

	Если ТекВидОперации = "Заказы" Тогда
		тДокОснование = "	ВЫБОР КОГДА Док.ДокументОснование ССЫЛКА Документ.ЗаказПокупателя
						|				И Док.ДокументОснование <> Значение(Документ.ЗаказПокупателя.ПустаяСсылка)
						|			ИЛИ Док.ДокументОснование ССЫЛКА Документ.ПоступлениеТоваров
						|				И Док.ДокументОснование <> Значение(Документ.ПоступлениеТоваров.ПустаяСсылка)
