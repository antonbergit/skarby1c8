			СообщениеПользователю(РезультатВозврата.ТелоОтвета, Ложь);
			Возврат
		КонецЕсли;
		Если Найти(РезультатВозврата.Статус, "200") = 0 Тогда
			Возврат
		КонецЕсли
	Исключение
	КонецПопытки;
	ТекстовыйДокумент = Новый ТекстовыйДокумент;
	Если ЗначениеЗаполнено(ВыхФайлГС.ПолноеИмя) Тогда
		ТекстовыйДокумент.Прочитать(ВыхФайлГС.ПолноеИмя);
		ТекстГС = ТекстовыйДокумент.ПолучитьТекст();
	Иначе
