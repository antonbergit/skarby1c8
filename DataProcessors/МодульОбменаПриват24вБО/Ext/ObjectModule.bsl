	ТекстВидаОбъектаРППусто = НСтр("ru='Отсутствуют карты';uk='Відсутні карти'");
	стРезультат = Новый Структура;
	ТекстСообщения = "";
	НомерПачкиКарты = 1;

	Если НЕ УспешноПройденыАвторизационныеЭтапыВ3() Тогда
		стРезультат.Вставить("ТекстСообщения", ТекстСообщения);
		стРезультат.Вставить("Успешно", Ложь);
		Возврат стРезультат
	КонецЕсли;

	Если ДействиеАналитики = "ОтчетПоКартам" Тогда
		ТабличноеПолеОтчет.Очистить()
	Иначе
		ТабличноеПолеКорпКарты.Очистить();
		тзКорпКарты.Очистить()
