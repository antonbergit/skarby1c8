			КонецЕсли
		Исключение
			Попытка
				//	Виписка за групою карток
				мКорпорации = СтруктураКорпорацийИКарт.CorpStatement;
				Если Не ЗначениеЗаполнено(мКорпорации) Или Не ЗначениеЗаполнено(мКорпорации.statements) Тогда
					стРезультат.Вставить("ТекстСообщения", ТекстВидаОбъектаРППусто);
					стРезультат.Вставить("Успешно", Ложь);
					Возврат стРезультат
				КонецЕсли
			Исключение	
				стРезультат.Вставить("ТекстСообщения", ТекстВидаОбъектаРППусто);
				стРезультат.Вставить("Успешно", Ложь);
				Возврат стРезультат
			КонецПопытки
		КонецПопытки
	КонецПопытки;

	Если ДействиеАналитики = "ВыпискаПоКартам" Тогда
		Попытка
			//	карта
			ФИОДержателяКарты = мКорпорации.summary.holderValue;
			ЭтоГруппаКарт = Ложь
		Исключение
			//	группа карт
