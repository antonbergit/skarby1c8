		Исключение
		КонецПопытки;

		Попытка
			message = СтруктураОтвета.message
		Исключение
			message = ""
		КонецПопытки;

		Попытка
			codeSSO = СтруктураОтвета.requestId;
		Исключение
			codeSSO = ""
		КонецПопытки;

		Попытка
			type = СтруктураОтвета.code
		Исключение
			Попытка// не используется?
				type = СтруктураОтвета.descr
			Исключение
			КонецПопытки;
		КонецПопытки;

		Попытка
			code = СтруктураОтвета.code//поменять местами
		Исключение
			Попытка// не используется?
				code = СтруктураОтвета.ТекстОтветаmessage;
			Исключение
			КонецПопытки;
		КонецПопытки;

		Попытка
			codeTrace = СтруктураОтвета.serviceCode
		Исключение
