							СтрокаТЧ.ИдентификаторКартинки = 2;
						КонецПопытки;	
					Иначе
						ОбъектДокумента.Записать(РежимЗаписиДокумента.Запись);
						СтрокаТЧ.ТекстОшибки = "";
