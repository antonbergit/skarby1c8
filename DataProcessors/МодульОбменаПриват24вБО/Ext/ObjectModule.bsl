	РесурсНаСервереСлужебный = "/api/mop24/get_actual_version_info/8283/";
	ФайлГС = "Privat24Chm.txt";
	РесурсНаСервереГорячееСообщение = РесурсНаСервереСлужебный + ФайлГС;
	ФайлГСНаСервере = Сред(РесурсНаСервереГорячееСообщение, 2);
	//POST = Истина;УдалитьBOM = Ложь;

