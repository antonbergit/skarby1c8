Процедура ЗаполнитьЦенуИСуммыДокумента(СуммаВключаетНДС, НоваяСтрокаТЧ, СтрокаДЗ, ПодСтрокаДЗ, ЕстьНДС, CтавкаПДВ)

	Если ИдКонф = "UABY" Тогда

		Если НоваяСтрокаТЧ.Цена = 0 Тогда 
			НоваяСтрокаТЧ.Цена = ПолучитьЗначениеПоДеревуИзXML("Ціна", СтрокаДЗ)
		КонецЕсли;
		НоваяСтрокаТЧ.СуммаБезНДС = ПолучитьЗначениеПоДеревуИзXML("СумаБезПДВ", ПодСтрокаДЗ);
		НоваяСтрокаТЧ.СуммаСНДС = ПолучитьЗначениеПоДеревуИзXML("Сума", ПодСтрокаДЗ);
		НоваяСтрокаТЧ.СуммаНДС = ПолучитьЗначениеПоДеревуИзXML("СумаПДВ", ПодСтрокаДЗ);
		ПредставлениеСтавкиНДС = ПолучитьСтавкуНДСПоСтроковомуПредставлению(ПолучитьЗначениеПоДеревуИзXML("CтавкаПДВ", СтрокаДЗ));
		НоваяСтрокаТЧ.СтавкаНДС = ПредставлениеСтавкиНДС;
		Попытка
			Если ПредставлениеСтавкиНДС = "0" Тогда
				НоваяСтрокаТЧ.СтавкаНДС = Перечисления.СтавкиНДС.БезНДС
			Иначе
				НоваяСтрокаТЧ.СтавкаНДС = Перечисления.СтавкиНДС["НДС" + ПредставлениеСтавкиНДС]
			КонецЕсли
		Исключение
			НоваяСтрокаТЧ.СтавкаНДС = Перечисления.СтавкиНДС.НДС20
		КонецПопытки;
		Если СуммаВключаетНДС = Неопределено И ЗначениеЗаполнено(CтавкаПДВ) И CтавкаПДВ <> "0" Тогда
			СуммаВключаетНДС = Истина
		Иначе
			СуммаВключаетНДС = Ложь
		КонецЕсли

	ИначеЕсли СуммаВключаетНДС Тогда
		
		Если НоваяСтрокаТЧ.Цена = 0 Тогда 
			
			НоваяСтрокаТЧ.Цена = ПолучитьЗначениеПоДеревуИзXML("Ціна", СтрокаДЗ);
			
		Иначе
			
			Если ЕстьНДС Тогда
				Если Найти(CтавкаПДВ, "20") > 0 Тогда
					НоваяСтрокаТЧ.Цена = НоваяСтрокаТЧ.Цена + НоваяСтрокаТЧ.Цена * 0.2
				ИначеЕсли Найти(CтавкаПДВ, "14") > 0 Тогда
					НоваяСтрокаТЧ.Цена = НоваяСтрокаТЧ.Цена + НоваяСтрокаТЧ.Цена * 0.14
				ИначеЕсли Найти(CтавкаПДВ, "7") > 0 Тогда
					НоваяСтрокаТЧ.Цена = НоваяСтрокаТЧ.Цена + НоваяСтрокаТЧ.Цена * 0.07
				ИначеЕсли Найти(CтавкаПДВ, "0") > 0 Тогда
				Иначе
					ТекстСообщения = НСтр("ru = 'Неустановленная СтавкаНДС '; uk='Невстановлена СтавкаПДВ '");
					СообщениеПользователю(ТекстСообщения);
				КонецЕсли
			КонецЕсли;	
			
		КонецЕсли;
		
		НоваяСтрокаТЧ.Сумма = ПолучитьЗначениеПоДеревуИзXML("Сума", ПодСтрокаДЗ);
		
		Если НоваяСтрокаТЧ.Сумма = 0 Тогда
			
			НоваяСтрокаТЧ.Сумма = ПолучитьЗначениеПоДеревуИзXML("СумаБезПДВ", ПодСтрокаДЗ);	
			
			Если ЕстьНДС Тогда
				Если Найти(CтавкаПДВ, "20") > 0 Тогда
					НоваяСтрокаТЧ.Сумма = НоваяСтрокаТЧ.Сумма + НоваяСтрокаТЧ.Сумма * 0.2
				ИначеЕсли Найти(CтавкаПДВ, "14") > 0 Тогда
					НоваяСтрокаТЧ.Сумма = НоваяСтрокаТЧ.Сумма + НоваяСтрокаТЧ.Сумма * 0.14
				ИначеЕсли Найти(CтавкаПДВ, "7") > 0 Тогда
					НоваяСтрокаТЧ.Сумма = НоваяСтрокаТЧ.Сумма + НоваяСтрокаТЧ.Сумма * 0.07
				ИначеЕсли Найти(CтавкаПДВ, "0") > 0 Тогда
				Иначе
					ТекстСообщения = НСтр("ru = 'Неустановленная СтавкаНДС '; uk='Невстановлена СтавкаПДВ '");
					СообщениеПользователю(ТекстСообщения);
				КонецЕсли
			КонецЕсли;	
			
		КонецЕсли;
		
	Иначе//Если Не СуммаВключаетНДС Тогда
		
		//на текущий момент (21.01.2016) возможен вариант когда документ создан на сайте, 
		//и полей связанных с НДС по строкам вообще не будет. Для такого случая будем все вычислять
		
		НоваяСтрокаТЧ.Сумма = ПолучитьЗначениеПоДеревуИзXML("СумаБезПДВ", ПодСтрокаДЗ);
		
		Если НоваяСтрокаТЧ.Цена = 0 Тогда //И ЕстьНДС Тогда //если цена = 0, а НДС по документу есть - скорее всего документ создан на сайте
			
			НоваяСтрокаТЧ.Цена = ПолучитьЗначениеПоДеревуИзXML("Ціна", СтрокаДЗ);
			НоваяСтрокаТЧ.Сумма = ПолучитьЗначениеПоДеревуИзXML("Сума", ПодСтрокаДЗ);
			
		//Иначе //если НДС нет - то и вычислять ничего не нужно
			//	НоваяСтрокаТЧ.Цена = ПолучитьЗначениеПоДеревуИзXML("Ціна", СтрокаДЗ);
			//	НоваяСтрокаТЧ.Сумма = ПолучитьЗначениеПоДеревуИзXML("Сума", ПодСтрокаДЗ);
			
		КонецЕсли;

		Если НоваяСтрокаТЧ.Сумма = 0 Тогда
			НоваяСтрокаТЧ.Сумма = ПолучитьЗначениеПоДеревуИзXML("Сума", ПодСтрокаДЗ)
		КонецЕсли;
		Если НоваяСтрокаТЧ.Сумма = 0 Тогда
			НоваяСтрокаТЧ.Сумма = Окр(НоваяСтрокаТЧ.Цена * НоваяСтрокаТЧ.Количество, 2)
		КонецЕсли

	КонецЕсли;	
	
	НоваяСтрокаТЧ.СуммаНДС = ПолучитьЗначениеПоДеревуИзXML("СумаПДВ", ПодСтрокаДЗ); 

	Если НоваяСтрокаТЧ.СуммаНДС = 0 И ЕстьНДС И Не СуммаВключаетНДС Тогда
		Попытка
			НоваяСтрокаТЧ.СуммаНДС = НоваяСтрокаТЧ.Сумма * 20 / 100;
			НоваяСтрокаТЧ.СуммаСНДС = НоваяСтрокаТЧ.Сумма + НоваяСтрокаТЧ.СуммаНДС
		Исключение КонецПопытки
	КонецЕсли;

	НоваяСтрокаТЧ.СтавкаНДС = ПолучитьСтавкуНДСПоСтроковомуПредставлению(ПолучитьЗначениеПоДеревуИзXML("CтавкаПДВ", СтрокаДЗ));
	
	// ставка НДС в ЭДО не обязательный параметр
	Если НЕ ЗначениеЗаполнено(НоваяСтрокаТЧ.СтавкаНДС) Тогда
		
		Если ЕстьНДС Тогда

			Если СуммаВключаетНДС Тогда
				ПредставлениеСтавкиНДС = Окр(НоваяСтрокаТЧ.СуммаНДС / НоваяСтрокаТЧ.Сумма, 2) * 100
			Иначе
				ПредставлениеСтавкиНДС = Окр(НоваяСтрокаТЧ.СуммаНДС / (НоваяСтрокаТЧ.Сумма + НоваяСтрокаТЧ.СуммаНДС), 2) * 100
			КонецЕсли;

			Если ИдКонф = "UNF" Или ИдКонф = "BSB" Или ИдКонф = "AAA" Тогда
				
				Попытка
					НоваяСтрокаТЧ.СтавкаНДС = Справочники.СтавкиНДС.НайтиПоНаименованию(ПредставлениеСтавкиНДС + "%")
				Исключение
					НоваяСтрокаТЧ.СтавкаНДС = Справочники.СтавкиНДС.ОсновнаяСтавкаНДС
				КонецПопытки
				
			ИначеЕсли ИдКонф = "BP" Или ИдКонф = "BASC" ИЛИ ИдКонф = "UT_3" ИЛИ ИдКонф = "ERP" Или ИдКонф = "BASR" Или ИдКонф = "OSMD" Тогда 	

				Попытка
					НоваяСтрокаТЧ.СтавкаНДС = Перечисления.СтавкиНДС["НДС" + ПредставлениеСтавкиНДС]
				Исключение
					НоваяСтрокаТЧ.СтавкаНДС = Перечисления.СтавкиНДС.НДС20
				КонецПопытки

			КонецЕсли;	
			
		Иначе
			
			Если ИдКонф = "UNF" Или ИдКонф = "BSB" Или ИдКонф = "AAA" Тогда
				
				НоваяСтрокаТЧ.СтавкаНДС = Справочники.СтавкиНДС.НайтиПоНаименованию("Без НДС");	
				
			ИначеЕсли ИдКонф = "BP" Или ИдКонф = "BASC" ИЛИ ИдКонф = "UT_3" ИЛИ ИдКонф = "ERP" Или ИдКонф = "BASR" Или ИдКонф = "OSMD" Тогда 	
				
				НоваяСтрокаТЧ.СтавкаНДС = Перечисления.СтавкиНДС.БезНДС; 
				
			КонецЕсли;	
			
		КонецЕсли;
		
	КонецЕсли;
	
	Если НоваяСтрокаТЧ.СуммаНДС = 0 И ЕстьНДС Тогда //опять же скорее всего такой документ создан на сайте
		Если ИдКонф = "UABY" Тогда
			НоваяСтрокаТЧ.СуммаНДС = ПолучитьСуммуНДС(НоваяСтрокаТЧ.СуммаБезНДС, НоваяСтрокаТЧ.СтавкаНДС, Истина)
		Иначе
			НоваяСтрокаТЧ.СуммаНДС = ПолучитьСуммуНДС(НоваяСтрокаТЧ.Сумма, НоваяСтрокаТЧ.СтавкаНДС, СуммаВключаетНДС)
		КонецЕсли
	КонецЕсли;
	
КонецПроцедуры

