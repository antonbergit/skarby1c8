	// если не успешно сообщим ошибку
	Если Не UnitТест И Статус = 504 Тогда
		ТекстСообщения = НСтр("ru='Проверьте качество интернет-подключения или установите более узкий интервал дат и счетов';uk='Перевірте якість интернет-з`єднання або встановіть більш вузький інтервал дат та рахунків'");
		СообщениеПользователю(ТекстСообщения, Ложь);
		Возврат Ложь

	ИначеЕсли Не UnitТест И Найти("500,501", Статус) > 0 Тогда
		ТекстСообщения = НСтр("ru='Внутренняя ошибка сервиса.';uk='Внутрішня помилка сервісу.'");
		СообщениеПользователю(ТекстСообщения, Ложь);
		Возврат Ложь
	ИначеЕсли Не UnitТест И Статус = 400 Тогда
		ТекстСообщения = НСтр("ru='Ошибка в данных, проверьте правильность заполнения документа.';uk='Помилка в даних, перевірте правильність заповнення документа.'");
		СообщениеПользователю(ТекстСообщения, Ложь);
		ТекстОшибки = АнализИВыводОшибокАК(Статус, ТелоОтвета);
		Если ЗначениеЗаполнено(ТекстОшибки) Тогда
			СообщениеПользователю(ТекстОшибки, Ложь)
		КонецЕсли;
		Возврат Ложь

	ИначеЕсли Не UnitТест И Статус = 401 Тогда
		ТекстСообщения = НСтр("ru='Неправильные id и/или token автоклиента';uk='Невірні id та/або token автоклієнта'");
		СообщениеПользователю(ТекстСообщения, Ложь);
		ТекстОшибки = АнализИВыводОшибокАК(Статус, ТелоОтвета);
		Если ЗначениеЗаполнено(ТекстОшибки) Тогда
			СообщениеПользователю(ТекстОшибки, Ложь)
		КонецЕсли;
		Возврат Ложь

	ИначеЕсли Не UnitТест И Статус <> 201 И Статус <> 200 Тогда    		
		ТекстСообщения = НСтр("ru = 'Ошибка отправки запроса на экспорт платежных документов!';uk='Помилка надсилання запиту на експорт платіжних документів!'")
			+ Символы.ПС + "ТелоОтвета = " + ТелоОтвета;
		СообщениеПользователю(ТекстСообщения);
		ТекстОшибки = АнализИВыводОшибокАК(Статус, ТелоОтвета);
		Если ЗначениеЗаполнено(ТекстОшибки) Тогда
			СообщениеПользователю(ТекстОшибки, Ложь)
		КонецЕсли;
		Возврат Ложь;
	КонецЕсли;
	
	Возврат Истина; 
		  
КонецФункции

