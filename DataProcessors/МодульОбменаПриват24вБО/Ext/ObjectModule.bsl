							ТекстСообщения = НСтр("ru = 'Ошибка при проведении документа '; uk='Помилка при проведенні документа '") + ОбъектДокумента
								+ " " + ОписаниеОшибки();
							СообщениеПользователю(ТекстСообщения);
							СтрокаТЧ.ТекстОшибки = ТекстСообщения;
							СтрокаТЧ.КартинкаОшибки = 0;
							
