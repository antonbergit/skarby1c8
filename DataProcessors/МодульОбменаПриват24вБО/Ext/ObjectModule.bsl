		ТекстСообщения = стРезультат.ТекстСообщения;
		Если Не UnitТест Тогда
			СообщениеПользователю(ТекстСообщения, Ложь)
		КонецЕсли;
		Возврат
	КонецЕсли;

	Попытка
		СтруктураЗПdata = СтруктураЗП.data
	Исключение
		ТекстОписаниеОшибки = ОписаниеОшибки();
