				ТекстСообщения = НСтр("ru='В строке %рядок% не корректно заполнены данные платежа. Запрещено создание налоговых платежей без заполнения обязательных одного из структурированных полей для МФО 899998 и назначения платежа. Исправьте пожалуйста.';uk='В рядку %рядок% некоректно заповнені дані платежу. Заборонено створення податкових платежів без заповнення обов`язкового одного з структурованих полів для МФО 899998 та призначення платежу. Виправте будь ласка.'");
				ТекстСообщения = СтрЗаменить(ТекстСообщения, "%рядок%", СтрокаЭкспорта.НомерСтроки);
				СообщениеПользователю(ТекстСообщения, Ложь);
				Возврат Ложь
			КонецЕсли
		КонецЕсли;
	КонецЕсли;

	УдалитьКавычки = Истина;//Экранировать2Кавычки = Не ФлажокИспользоватьКурлВсегда;

	Назначение = ЭкранироватьJSON(СтрокаЭкспорта.НазначениеПлатежа, УдалитьКавычки);//Экранировать2Кавычки
	НаимКонтрагента = ЭкранироватьJSON(СтрокаЭкспорта.НаименованиеКонтрагента, УдалитьКавычки);//Экранировать2Кавычки
	
	СтрокаЗапроса = "{""document_number"":""" + ЭкранироватьJSON(ЗаменитьСС(СтрокаЭкспорта.НомерПоручения), УдалитьКавычки) + """, " 
		+ ПолучитьПараметрСчетаАК(СтрокаЭкспорта.НомерСчетаКонтрагента)
		+ """recipient_nceo"":""" + СтрокаЭкспорта.КодКонтрагента
		+ """, ""payment_naming"":""" + НаимКонтрагента
		+ """, ""payment_amount"":""" + СтрокаЭкспорта.Сумма
		+ """, ""recipient_ifi"":""" + СтрокаЭкспорта.КодБанкаКонтрагента
		//+ """, ""recipient_ifi_text"":""" + ЗаменитьСС(СокрЛП(СчетКонтрагента.Банк.Наименование))
		+ """, ""payment_destination"":""" + Назначение
