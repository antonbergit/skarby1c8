			тзНайденныйСотрудник.Колонки.Добавить("Ссылка")
		КонецПопытки
	КонецПопытки;

	Возврат тзНайденныйСотрудник

КонецФункции

Функция НайтиСотрудниковПоТНИННФИО(tabn, inn, fio)

	Попытка
		Запрос = Новый Запрос;
		Запрос.УстановитьПараметр("tabn", tabn);
		Запрос.УстановитьПараметр("inn", inn);
		Запрос.УстановитьПараметр("fio", fio);
		Запрос.Текст = "
			|ВЫБРАТЬ
			|	Сотрудники.Ссылка
			|ИЗ Справочник.Сотрудники КАК Сотрудники
			|ГДЕ Сотрудники.Код = &tabn
			|		ИЛИ Сотрудники.ФизическоеЛицо.КодПоДРФО = &inn
			|
			|ОБЪЕДИНИТЬ
			|
			|ВЫБРАТЬ
			|	Сотрудники.Ссылка
			|ИЗ Справочник.Сотрудники КАК Сотрудники
			|ГДЕ Сотрудники.Наименование = &fio";
		тзНайденныйСотрудник = Запрос.Выполнить().Выгрузить();
