			CтавкаПДВ = ПолучитьЗначениеПоДеревуИзXML("CтавкаПДВ", СтрокаДЗ);
			
			ТипНоменклатуры = ?(ЭтоАкт, "Послуга", "Товар");
			
			СтруктураПараметров = Новый Структура;
			СтруктураПараметров.Вставить("Артикул", АртикулНом);
			СтруктураПараметров.Вставить("Наименование", НаименованиеНом);
			СтруктураПараметров.Вставить("ЕдИзм", ЕдИзм);
			СтруктураПараметров.Вставить("Тип", ТипНоменклатуры);
			СтруктураПараметров.Вставить("CтавкаПДВ", CтавкаПДВ);
			
			//Если ТипНоменклатуры = "Послуга" Тогда
			Если ЭтоАкт Тогда
				НоваяСтрокаТЧ = НовыйДок.Услуги.Добавить();	
			Иначе
				НоваяСтрокаТЧ = НовыйДок.ТМЦ.Добавить();	
				//НоваяСтрокаТЧ.ЕдиницаИзмерения = НайтиСоздатьЕдиницуИзмерения(ЕдИзм);	
			КонецЕсли;
			
			НоваяСтрокаТЧ.Объект = НайтиСоздатьНоменклатуру(СтруктураПараметров, Истина);
			НоваяСтрокаТЧ.Количество = ПолучитьЗначениеПоДеревуИзXML("ЗаявленаКількість", СтрокаДЗ);
			
			ПараметрыПоиска = Новый Структура;
			ПараметрыПоиска.Вставить("НазваниеУзла", "ВсьогоПоРядку");
			НайденныеПодСтроки = СтрокаДЗ.Строки.НайтиСтроки(ПараметрыПоиска, Истина);
			
			НоваяСтрокаТЧ.Цена = ПолучитьЗначениеПоДеревуИзXML("БазоваЦіна", СтрокаДЗ);
			
			//если тег "Базовая цена" пустой - это значит что документ создавался или не сайте или другим приложением
			//в наших обработках он везеде заполняется
			//Если НоваяСтрокаТЧ.Цена = 0 Тогда
			//	НовыйДок.СуммаВключаетНДС = Ложь;	
			//КонецЕсли;
			
			Для Каждого ПодСтрокаДЗ Из НайденныеПодСтроки Цикл	

				ЗаполнитьЦенуИСуммыДокумента(Неопределено, НоваяСтрокаТЧ, СтрокаДЗ, ПодСтрокаДЗ, ЕстьНДС, CтавкаПДВ)
				
			КонецЦикла;
			
		КонецЦикла;	

	КонецЕсли;//Если ЗначениеЗаполнено(СтрокаТЧ.XML) Тогда

	ЗаписатьДокумент(НовыйДок, СтрокаТЧ);
	//ПросмотрДокЭДО
	////НайтиДобавитьтзНовыйДокФин(НовыйДок, ДобавитьДок, СтрокаТЧ.НомерСтроки)

КонецПроцедуры

// БУ, УПП, УТП, ТЧП, ТЧПБ, ОП, УТ2, ББУ, КУБУ// AA, ЗУП, ЗиКБУ, UAH, ОСМД не используется,, БУСХП, УАБ,,, УТ3
Функция НайтиДокументПідстави(ТипДокументуПідстави, КодТипуДокументуПідстави, НомерДокументуПідстави, ДатаДокументуПідстави, СтрокаТЧ,
		СтруктураДоговора)

	// UNF, BSB, BP, BASC, AAA, UABY, UT_3, ERP, BASR,,, OSMD,KUBU, //UT_2,UTP,UPP,ZUP
	Перем ДокуменСсылкаВБазе;

	СуммаДокумента = СтрокаТЧ.СуммаДокумента;
	Контрагент = СтрокаТЧ.Контрагент;
	Покупатель = Ложь;

	Если Найти("UABY", ИдКонф) > 0 И (КодТипуДокументуПідстави = "001" Или КодТипуДокументуПідстави = "1") Тогда
		// 001 - Договор
		ДокуменСсылкаВБазе = НайтиСоздатьДоговорКонтрагента(Контрагент, Покупатель, СтруктураДоговора)

	ИначеЕсли КодТипуДокументуПідстави = "006" Или КодТипуДокументуПідстави = "6" Тогда
	// ИСХодящие док
		// 006 - Видаткова накладна
		ДокуменСсылкаВБазе = ЗаполнитьСсылкуНаНайденныйФинДокПідстави("РеализацияТоваровУслуг", НомерДокументуПідстави, ДатаДокументуПідстави, СуммаДокумента, Контрагент)

	// ВХодящие док
	ИначеЕсли КодТипуДокументуПідстави = "007" Или КодТипуДокументуПідстави = "7" Тогда
		// 007 - Прибуткова накладна
		ДокуменСсылкаВБазе = ЗаполнитьСсылкуНаНайденныйФинДокПідстави("ПоступлениеТоваровУслуг", НомерДокументуПідстави, ДатаДокументуПідстави, СуммаДокумента, Контрагент)

	ИначеЕсли КодТипуДокументуПідстави = "013" Или КодТипуДокументуПідстави = "13" Тогда
		// 013 – Акт виконаних робіт
		ДокуменСсылкаВБазе = ЗаполнитьСсылкуНаНайденныйФинДокПідстави("ПоступлениеТоваровУслуг", НомерДокументуПідстави, ДатаДокументуПідстави, СуммаДокумента, Контрагент)

	ИначеЕсли КодТипуДокументуПідстави = "016" Или КодТипуДокументуПідстави = "16" Тогда
		// 016 – Рахунок-фактура
		ДокуменСсылкаВБазе = ЗаполнитьСсылкуНаНайденныйФинДокПідстави("СчетНаОплатуПоставщика", НомерДокументуПідстави, ДатаДокументуПідстави, СуммаДокумента, Контрагент)

	ИначеЕсли КодТипуДокументуПідстави = "018" Или КодТипуДокументуПідстави = "18" Тогда
		// 018 - Замовлення
		ДокуменСсылкаВБазе = ЗаполнитьСсылкуНаНайденныйФинДокПідстави("ЗаказПокупателя", НомерДокументуПідстави, ДатаДокументуПідстави, СуммаДокумента, Контрагент)

	//ИначеЕсли КодТипуДокументуПідстави = "019" Или КодТипуДокументуПідстави = "19" Тогда
	//	// 019 - Заявление на отпуск

	КонецЕсли;

	Возврат ДокуменСсылкаВБазе
	
КонецФункции

Функция ЗаполнитьСсылкуНаНайденныйФинДокПідстави(ТипФинДок, НомерДокументуПідстави, ДатаДокументуПідстави, СуммаДокумента, Контрагент)
	
	ДокуменСсылкаВБазе = Неопределено;
	
	СтруктураПоиска = Новый Структура;
	СтруктураПоиска.Вставить("ДатаВходящегоДокумента", ДатаДокументуПідстави);
	СтруктураПоиска.Вставить("НомерВходящегоДокумента", НомерДокументуПідстави);
	СтруктураПоиска.Вставить("СуммаДокумента", СуммаДокумента);
	СтруктураПоиска.Вставить("Контрагент", Контрагент);

	Если Найти("СчетНаОплатуПоставщика,Счета", ТипФинДок) > 0 Тогда//ТекВидОперации
		
		Если ИдКонф = "UNF" ИЛИ ИдКонф = "BP" Или ИдКонф = "BASC" Или ИдКонф = "BSB" Тогда
			ДокуменСсылкаВБазе = UNF_BP_НайтиСчетПоставщика(СтруктураПоиска);
		ИначеЕсли ИдКонф = "AAA" Тогда
			ДокуменСсылкаВБазе = AAA_НайтиСчетПоставщика(СтруктураПоиска)
		ИначеЕсли ИдКонф = "UABY" Тогда
			ДокуменСсылкаВБазе = UABY_НайтиСчетПоставщика(СтруктураПоиска);
			// ERP счета поставщика нет
		КонецЕсли;
		
	ИначеЕсли Найти("ПоступлениеТоваровУслуг,Акты", ТипФинДок) > 0 Тогда//ТекВидОперации 
		
		Если ИдКонф = "UNF" Или ИдКонф = "BSB" Тогда
			
			ДокуменСсылкаВБазе = UNF_НайтиПриходнуюНакдадную(СтруктураПоиска);
			
		ИначеЕсли ИдКонф = "BP" Или ИдКонф = "BASC" Тогда
		
			ДокуменСсылкаВБазе = BP_НайтиПриходнуюНакдадную(СтруктураПоиска);
			
		ИначеЕсли ИдКонф = "UT_3" ИЛИ ИдКонф = "ERP" Тогда
		
			ДокуменСсылкаВБазе = UT_3_НайтиПриходнуюНакдадную(СтруктураПоиска);

		ИначеЕсли ИдКонф = "BASR" Тогда
			ДокуменСсылкаВБазе = BASR_НайтиПриходнуюНакдадную(СтруктураПоиска);

		ИначеЕсли ИдКонф = "AAA" Тогда
			ДокуменСсылкаВБазе = AAA_НайтиПриходнуюНакдадную(СтруктураПоиска);
		ИначеЕсли ИдКонф = "UABY" Тогда
			ДокуменСсылкаВБазе = UABY_НайтиПриходнуюНакдадную(СтруктураПоиска);
		КонецЕсли;
		
	ИначеЕсли Найти("ЗаказПокупателя,Заказы", ТипФинДок) > 0 Тогда//ТекВидОперации 
		
		Если ИдКонф = "UNF" Или ИдКонф = "BSB" Тогда
			
			ДокуменСсылкаВБазе = UNF_НайтиЗаказПокупателя(СтруктураПоиска);
			
		ИначеЕсли ИдКонф = "UT_3" ИЛИ ИдКонф = "ERP" Тогда
			
			ДокуменСсылкаВБазе = UT_3_НайтиЗаказПокупателя(СтруктураПоиска);

		ИначеЕсли ИдКонф = "BASR" Тогда
			ДокуменСсылкаВБазе = BASR_НайтиЗаказПокупателя(СтруктураПоиска);

		ИначеЕсли ИдКонф = "AAA" Тогда
			ДокуменСсылкаВБазе = AAA_НайтиЗаказПокупателя(СтруктураПоиска);
		КонецЕсли;

	ИначеЕсли Найти("РеализацияТоваровУслуг,Расходные", ТипФинДок) > 0 Тогда//ТекВидОперации

		ДокуменСсылкаВБазе = BU_НайтиРасходнуюНакдадную(СтруктураПоиска)

	КонецЕсли;

	Возврат ДокуменСсылкаВБазе

КонецФункции	

Функция BU_НайтиРасходнуюНакдадную(СтруктураПоиска)

	Попытка
		// BASC, UT_3, BASIEM, ERP, остальные
		ИмяДок = "РеализацияТоваровУслуг";
		СсылкаНаДокумент = Документы[ИмяДок].ПустаяСсылка()
	Исключение
		Попытка
			// UNF, BSB
			ИмяДок = "РасходнаяНакладная";
			СсылкаНаДокумент = Документы[ИмяДок].ПустаяСсылка()
		Исключение
			Попытка
				// OSMD
				ИмяДок = "РасходныйОрдерНаЗапасы";
				СсылкаНаДокумент = Документы[ИмяДок].ПустаяСсылка()
			Исключение
				Попытка
					// UABY
					ИмяДок = "РеализацияУслуг";
					СсылкаНаДокумент = Документы[ИмяДок].ПустаяСсылка()
				Исключение
					// AAA, BASR
					ИмяДок = "РеализацияТоваров";
					СсылкаНаДокумент = Документы[ИмяДок].ПустаяСсылка()
				КонецПопытки
			КонецПопытки
		КонецПопытки
	КонецПопытки;

	Запрос = Новый Запрос;
	Запрос.Текст = 
		"ВЫБРАТЬ РАЗРЕШЕННЫЕ
		|	РасходнаяНакладная.Ссылка
		|ИЗ
		|	Документ." + ИмяДок + " КАК РасходнаяНакладная
		|ГДЕ
		|	РасходнаяНакладная.Контрагент = &Контрагент
		|	И РасходнаяНакладная.СуммаДокумента = &СуммаДокумента
		|" + ?(Найти("UNF,BSB", ИдКонф) > 0, "
		|	И РасходнаяНакладная.НомерВходящегоДокумента = &НомерВходящегоДокумента
		|	И РасходнаяНакладная.ДатаВходящегоДокумента = &ДатаВходящегоДокумента
		|", "
		|	И РасходнаяНакладная.Номер = &НомерВходящегоДокумента
		|	И РасходнаяНакладная.Дата = &ДатаВходящегоДокумента");

	Запрос.УстановитьПараметр("ДатаВходящегоДокумента", СтруктураПоиска.ДатаВходящегоДокумента);
	Запрос.УстановитьПараметр("НомерВходящегоДокумента", СтруктураПоиска.НомерВходящегоДокумента);
	Запрос.УстановитьПараметр("Контрагент", СтруктураПоиска.Контрагент);
	Запрос.УстановитьПараметр("СуммаДокумента", СтруктураПоиска.СуммаДокумента);
	
	РезультатЗапроса = Запрос.Выполнить();
	
	ВыборкаДетальныеЗаписи = РезультатЗапроса.Выбрать();
	
	Пока ВыборкаДетальныеЗаписи.Следующий() Цикл
		СсылкаНаДокумент = ВыборкаДетальныеЗаписи.Ссылка;
	КонецЦикла;
	
	Возврат СсылкаНаДокумент
	
КонецФункции

