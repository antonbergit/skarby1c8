Функция ПоискСотрудниковПоТНиИНН(ТНСотр, inn, тзПоиска, Сотрудник = Неопределено)

	// 1. Поиск по ТН и ИНН
	ПараметрыОтбора = Новый Структура;
	ПараметрыОтбора.Вставить("tabn", ТНСотр);
	ПараметрыОтбора.Вставить("inn", inn);
	Если Сотрудник <> Неопределено Тогда
		ПараметрыОтбора.Вставить("Сотрудник", Сотрудник)
	КонецЕсли;
	мНайденныйСотрудникПЗ = тзПоиска.НайтиСтроки(ПараметрыОтбора);

	Если мНайденныйСотрудникПЗ.Количество() = 0 Тогда
		// 2. Поиск по ТН
		ПараметрыОтбора = Новый Структура;
		ПараметрыОтбора.Вставить("tabn", ТНСотр);
		мНайденныйСотрудникПЗ = тзПоиска.НайтиСтроки(ПараметрыОтбора)
	КонецЕсли;
		
	Если мНайденныйСотрудникПЗ.Количество() = 0 Тогда
		// 3. Поиск по ИНН
		ПараметрыОтбора = Новый Структура;
		ПараметрыОтбора.Вставить("inn", inn);
		мНайденныйСотрудникПЗ = тзПоиска.НайтиСтроки(ПараметрыОтбора)
	КонецЕсли;

	Возврат мНайденныйСотрудникПЗ

