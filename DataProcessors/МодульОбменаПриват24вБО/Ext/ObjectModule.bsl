				+ " Статус = " + Статус + Символы.ПС + ТелоОтвета;
			СообщениеПользователю(ТекстСообщения);
			ЕстьОшибки = Истина;
			Если Статус = 400 Тогда
				Возврат ТелоОтвета
			Иначе
				Продолжить
			КонецЕсли
		КонецЕсли;

		Если ТелоОтвета = "[]" Тогда
			ДокументПодписан = Ложь
		Иначе
			ДокументПодписан = Истина
		КонецЕсли;
		ТекстСообщения = НСтр("ru='Документ, отмеченный в строке №'; uk='Документ, зазначений у рядку №'")
