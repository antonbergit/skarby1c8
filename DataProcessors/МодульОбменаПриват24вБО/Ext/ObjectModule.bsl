    Стр = СтрЗаменить(Стр,"\","\\");
    Стр = СтрЗаменить(Стр,Символы.ПС,"\n");
    Стр = СтрЗаменить(Стр,Символы.ВК,"\r");
    Стр = СтрЗаменить(Стр,Символы.Таб,"\t");
    Стр = СтрЗаменить(Стр,"/","");//Стр = СтрЗаменить(Стр,"/","\/");
	//Если ЭкранироватьДвойныеКавычки Тогда
    Стр = СтрЗаменить(Стр,"""","\""");
	//КонецЕсли;

    Возврат Стр;
	
