			parameter = СтруктураОтвета.parameter
		Исключение
			parameter = ""
		КонецПопытки;

	Иначе
		//	xml	//	ошибки транзакций и остатков (выписки)
		type = спцПрочитатьУзел("type", ТекстОтвета);
